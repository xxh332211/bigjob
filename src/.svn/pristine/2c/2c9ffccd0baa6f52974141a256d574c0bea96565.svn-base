//取消商品收藏
function unfollowProduct(obj) {
    if (confirm("是否取消关注")) {
        var id = obj.data("id");
        $.ajax({
            type: "GET",
            url: domain + "/member/cancelcollectproduct.html",
            dataType: "json",
            async: false,
            data: {
                productId: id
            },
            success: function (data) {
                if (data.success) {
                    obj.parent().remove();
                } else {
                    alerts(data.message);
                }
            },
            error: function () {
                alerts("异常，请重试！");
            }
        });
    }
}

//删除浏览记录
function deleteProductId(obj) {
    var productId = obj.data("id");
    $.ajax({
        type: "GET",
        url: domain + "/member/deleteproductlog.html",
        data: {
            productId: productId
        },
        dataType: "json",
        success: function (data) {
            if (data.success) {
                obj.parent().remove();
            }
        }
    });
}
