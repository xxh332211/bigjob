//取消订单
function cancalOrder(tradeNo) {
    if (confirm("确定要取消该订单吗？")) {
        $.ajax({
            type: "GET",
            url: domain + "/member/cancalorder.html",
            data: {
                tradeNo: tradeNo
            },
            dataType: "json",
            success: function (data) {
                if (data.success) {
                    window.location.reload();
                } else {
                    alerts(data.message);
                }
            },
            error: function () {
                alerts("数据加载失败！");
            }
        });
    }
}

