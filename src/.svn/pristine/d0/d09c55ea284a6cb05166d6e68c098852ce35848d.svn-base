.login_head {
	height: 90px;
	padding-top: 5px;
	padding-bottom: 5px;
}

.login_head .pic {
	width: 350px;
	height: 80px;
	float: left;
	background: pink;
	vertical-align: middle;
}

.login_head .title {
	float: left;
	margin-left: 15px;
	margin-top: 23px;
}

.login_head .title p {
	font-size: 24px;
	color: #333333;
}

.login_content {
	background: #ffe6d0;
	height: 600px;
}

.login_content .login_center .pic {
	margin-top: 120px;
	margin-left: 200px;
}

.login_content .login_center .login_form {
	margin-top: 90px;
	background: #ffffff;
	box-shadow: 0 0 3px 0 #e8e8e8;
	border-radius: 6px;
	width: 370px;
	padding: 20px;
	padding-bottom: 65px;
	position: relative;
}

.login_content .login_center .login_form .title {
	font-size: 20px;
	color: #ff7e18;
	margin-bottom: 36px;
	line-height: 1em;
}

.login_content .login_center .login_form .double_title {
	line-height: 1em;
	margin-bottom: 36px;
}

.login_content .login_center .login_form .double_title .buyer_title {
	font-size: 20px;
	color: #ff7e18;
	float: left;
	margin-left: 35px;
	cursor: pointer;
}

.login_content .login_center .login_form .double_title .subline {
	border: 1px solid #ffe6d0;
	width: 1px;
	height: 25px;
	position: relative;
	top: -4px;
	float: left;
	margin-left: 30px;
	margin-right: 30px;
}

.login_content .login_center .login_form .double_title .supplyer_title {
	font-size: 20px;
	color: #333333;
	float: left;
	cursor: pointer;
}

.login_content .login_center .login_form .notice {
	background: #fff0f0;
	border: 1px solid #ff5554;
	width: 330px;
	height: 20px;
	position: absolute;
	line-height: 17px;
	top: 55px;
	left: 20px;
	padding-left: 10px;
}

.login_content .login_center .login_form .notice i {
	background: #ff3c23;
	width: 14px;
	height: 14px;
	border-radius: 100%;
	color: #fff;
	line-height: 14px;
	text-align: center;
	vertical-align: middle;
}

.login_content .login_center .login_form .notice .word {
	font-size: 10px;
	color: #ff3110;
}

.login_content .login_center .login_form .fullinput {
	position: relative;
	margin-bottom: 20px;
}

.login_content .login_center .login_form .fullinput.focus input {
	border-color: #27b9f2;
}

.login_content .login_center .login_form .fullinput.error input {
	border-color: #ff3c23;
}

.login_content .login_center .login_form .fullinput input {
	background: #ffffff;
	border: 1px solid #e5e5e5;
	width: 330px;
	height: 42px;
	text-indent: 60px;
}

.login_content .login_center .login_form .fullinput i {
	width: 20px;
	height: 19px;
	font-size: 19px;
	color: #cccccc;
	position: absolute;
	top: 12px;
	left: 12px;
}

.login_content .login_center .login_form .fullinput .getcode {
	position: absolute;
	top: 0;
	right: 0;
	margin-top: 2px;
	margin-right: 2px;
}

.login_content .login_center .login_form .fullinput .getcode button {
	background: #ff9c00;
	width: 82px;
	height: 38px;
	font-size: 12px;
	color: #ffffff;
	border: none;
	outline: none;
	cursor: pointer;
}

.login_content .login_center .login_form .fullinput .getcomfirm {
	position: absolute;
	right: 0;
	top: 0;
	background: #eeeeee;
	width: 82px;
	height: 38px;
	margin-top: 2px;
	margin-right: 2px;
	line-height: 38px;
	text-align: center;
	user-select: none;
}

.login_content .login_center .login_form .fullinput .getcomfirm span {
	font-size: 18px;
	color: #999999;
}

.login_content .login_center .login_form .check_protocol {
	user-select: none;
	margin-bottom: 20px;
}

.login_content .login_center .login_form .check_protocol p {
	margin-bottom: 3px;
}

.login_content .login_center .login_form .check_protocol p i {
	background: #ffffff;
	border: 1px solid #eeeeee;
	width: 16px;
	height: 16px;
	color: #fff;
	vertical-align: middle;
	margin-right: 5px;
	margin-top: -2px;
	cursor: pointer;
}

.login_content .login_center .login_form .check_protocol p:nth-child(1) {
	font-size: 12px;
	color: #666666;
	line-height: 1em;
}

.login_content .login_center .login_form .check_protocol p:nth-child(1) span {
	color: #ff7e18;
	cursor: pointer;
}

.login_content .login_center .login_form .check_protocol p:nth-child(2) {
	padding-left: 104px;
	font-size: 12px;
	color: #666666;
	line-height: 1em;
}

.login_content .login_center .login_form .check_protocol p:nth-child(2) span {
	color: #ff7e18;
	cursor: pointer;
}

.login_content .login_center .login_form .check_protocol.selected p i {
	color: #1ab4f2;
	font-weight: bold;
}

.login_content .login_center .login_form .login_btn button {
	background: #ff7e18;
	border-radius: 2px;
	width: 330px;
	height: 42px;
	color: #fff;
	line-height: 42px;
	text-align: center;
	font-size: 20px;
	color: #ffffff;
	border: none;
	outline: none;
	cursor: pointer;
}

.login_content .login_center .login_form .login_btn button:active {
	background: #f86f02;
}

.login_content .login_center .login_form .login_btn.disable button {
	background: #bdbdbd;
}

.login_content .login_center .login_form .tag {
	margin-top: 10px;
}

.login_content .login_center .login_form .tag a {
	font-size: 12px;
	color: #ff7e18;
}