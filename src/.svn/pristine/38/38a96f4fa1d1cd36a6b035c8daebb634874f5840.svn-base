.fl {
	float: left;
}

.fr {
	float: right;
}

.clearfix:after,
.clearfix:before {
	content: "";
	line-height: 0;
	height: 0;
	display: block;
	visibility: hidden;
	clear: both;
}

.clearfix {
	*zoom: 1;
}

.banxin {
	width: 1200px;
	margin: 0 auto;
	border: none;
}

.middle {
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	z-index: 99;
}

.center {
	position: absolute;
	left: 50%;
	transform: translateX(-50%);
	z-index: 99;
}

.hide {
	display: none;
}

.show {
	display: block;
}

.scal {
	overflow: hidden;
	position: relative;
}

.scal img {
	width: 100%;
	transition: width .3s ease-out;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.scal:hover img {
	width: 110%;
	transition: width .3s ease-out;
}

.jump {
	position: relative;
	top: 0;
	transition: all .1s;
}

.jump:hover {
	top: -10px;
	transition: all .1s ease-out;
}

.shake {
	overflow: hidden;
	position: relative;
}

.shake img {
	width: 100%;
	height: 100%;
	transition: all .3s ease-out;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.shake:hover img {
	transform: translate(-52%, -50%);
	transition: all .3s ease-out;
}

.cover_show {
	position: relative;
}

.cover_show .pic_cover_all {
	transition: all .4s ease-in;
	width: 100%;
	height: 100%;
}

.cover_show:hover .pic_cover_all {
	opacity: 1;
	transition: all .4s ease-in;
}

.cover_show .pic_cover_all {
	opacity: 0;
}

.cover_show .pic_cover_all .pic_cover {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.5);
	text-align: center;
}

.cover_show .pic_cover_all .pic_cover p {
	margin-top: 30px;
	font-size: 12px;
	color: #ffffff;
	line-height: 12px;
}

.cover_show .pic_cover_all .pic_cover a {
	display: inline-block;
	margin-top: 12px;
	background: #ff3c23;
	border-radius: 51px;
	width: 64px;
	height: 20px;
	line-height: 20px;
	color: #fff;
	cursor: pointer;
	font-size: 10px;
}

.tag_top {
	padding: 10px;
	width: 300px;
	height: 120px;
	border: 1px solid #bdbdbd;
	position: absolute;
	background: #ffffd5;
	/*设置圆角*/
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	left: 0;
	top: 0;
}

.tag_top:before,
.tag_top:after {
	content: "";
	display: block;
	border-width: 7px;
	position: absolute;
	top: -15px;
	left: 30px;
	border-style: solid dashed dashed solid;
	border-color: transparent  transparent #bdbdbd transparent;
	font-size: 0;
	line-height: 0;
}

.tag_top:after {
	top: -14px;
	border-color: transparent transparent #ffffd5 transparent;
}

