@charset "UTF-8";

* {
	margin: 0;
	padding: 0;
	/*在移动端的特殊设置*/
	/*点击高亮效果*/
	-webkit-tap-highlight-color: transparent;
	/*透明其实就是不显示颜色*/
	/*设置所有盒子的宽度以边框开始计算*/
	-webkit-box-sizing: border-box;
	/*要兼容webkit浏览器内核厂商 这种情况一般是老的移动端浏览器*/
	box-sizing: border-box;
}

body {
	font-size: 14px;
	color: #333;
	background: #fafafa;
	font-family: 'NotoSansHans', sans-serif;
	/*默认字体*/
}

ul,
ol,
dl,
li,
dt,
dd {
	list-style: none;
	padding: 0;
	margin: 0;
}

a {
	text-decoration: none;
	color: #666;
}

a:visited {
	color: #666;
	text-decoration: none;
}

a:hover {
	color: #666;
	text-decoration: none;
}

i,
em,
b {
	font-style: normal;
	font-weight: normal;
	display: inline-block;
	line-height: 1em;
}

input,
textarea {
	border: none;
	outline: none;
	resize: none;
	/*特殊的属性定义 清楚浏览器给input自带的样式。*/
	-webkit-appearance: none;
	/*组件默认的样式空*/
}

h1,
h2,
h3,
h4,
h5,
h6 {
	font-weight: normal;
	line-height: 1em;
}

label {
	display: inline-block;
	width: auto;
	margin-bottom: 0;
	font-weight: normal;
}

img {
	border: none;
	float: left;
}

.laydate_body .laydate_top {
	height: 35px !important;
}

.laydate_body .laydate_bottom {
	height: 35px !important;
}

.laydate_body .laydate_ym .laydate_yms {
	overflow: hidden;
}

.modal_cover,
.modal_cover_process {
	position: fixed;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.5);
	z-index: 999;
	text-align: center;
	line-height: 100%;
	top: 0;
	left: 0;
	overflow: hidden;
}

.modal_cover:before,
.modal_cover_process:before {
	content: '';
	display: inline-block;
	height: 100%;
	vertical-align: middle;
	margin-right: -0.25em;
}

.modal_cover .modal_cotainer,
.modal_cover_process .modal_cotainer {
	padding-top: 20px;
	padding-bottom: 20px;
	background: #fff;
	display: inline-block;
	vertical-align: middle;
	border-radius: 5px;
	position: relative;
}

.modal_cover .modal_cotainer .closes,
.modal_cover_process .modal_cotainer .closes {
	position: absolute;
	top: 10px;
	right: 10px;
	padding: 0;
	margin: 0;
	color: #ccc;
	cursor: pointer;
	z-index: 999;
}

.modal_cover .modal_cotainer .closes .iconfont,
.modal_cover_process .modal_cotainer .closes .iconfont {
	border-radius: 8px;
	cursor: pointer;
}

.modal_cover .modal_cotainer div,
.modal_cover_process .modal_cotainer div {
	padding-left: 20px;
	padding-right: 20px;
	margin-top: 20px;
	margin-bottom: 20px;
	text-align: left;
}

.modal_cover .modal_cotainer div.nomatch,
.modal_cover_process .modal_cotainer div.nomatch {
	position: absolute !important;
	bottom: -20px;
	left: 0;
	height: 20px;
	line-height: 20px;
	font-size: 10px;
	display: block;
	color: #ff3110;
}

.modal_cover .modal_cotainer div.nomatch .iconfont,
.modal_cover_process .modal_cotainer div.nomatch .iconfont {
	font-size: 10px;
	vertical-align: middle;
	height: 15px;
	width: 15px;
	line-height: 15px;
	display: inline-block;
	position: static;
	margin-left: 10px;
	color: #ff3110;
}

.modal_cover .modal_cotainer .title,
.modal_cover_process .modal_cotainer .title {
	font-size: 20px;
	color: #ff7e18;
	margin-bottom: 30px;
	margin-top: 0;
	font-weight: bold;
}

.modal_cover .modal_cotainer .alert,
.modal_cover_process .modal_cotainer .alert {
