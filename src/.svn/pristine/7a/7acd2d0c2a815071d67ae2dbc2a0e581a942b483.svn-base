@import '../../common/base';
@import '../../common/reset';
@import '../../component/foot';
@import '../../layout/quick_buy/ensure.scss';
.qrcode {
    height: 500px;
    position: relative;
    padding-top: 34px;
    padding-left: 223px;
    background : #fff;
    .title {
        font-size:20px;
        color:#4a4a4a;
        position: absolute;
        top: 27px;
        left: 78px;
    }
    .code {
        width: 300px;
        float: left;
        .code_pic {
            width: 300px;
            img {
                width: 100%;
            }
        }
        .tip {
            width: 300px;
            margin-top: 10px;
            width: 100%;
        }
    }
    .pic {
        width: 305px;
        float: left;
        margin-left:57px;
        img {
            width: 100%;
        }
    }
}
