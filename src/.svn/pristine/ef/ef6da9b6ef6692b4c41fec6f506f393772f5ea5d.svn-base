//location -s 
.location {
	margin-bottom: 20px;
	.location_single {
		margin-right: 15px;
		margin-top: 20px;
		&:last-child {
			margin-right: 0;
		}
		.location_head {
			width:390px;
			height:54px;
			position:relative;
			.seemore {
				position: absolute;
				top: 50%;
				transform:translateY(-50%);
				right:20px;
				span {
					font-size:14px;
					color:#ffffff;
					i {
						margin-left: 11px;
					}
				}
			}
		}
		.location_content {
			width: 390px;
			height:390px;
			border-right: 1px solid #e7e7e7;
			.location_product {
				width:194px;
				height:194px;
				padding-top:18px;
				border-left: 1px solid #e7e7e7;
				border-bottom: 1px solid #e7e7e7;
				.pic {
					width:140px;
					height:140px;
					border:1px solid black;
					margin-left:27px;
				}
				.price {
					text-align: center;
					span {
						color:#ff3c23;
						font-size: 14px;
					}
					del {
						color : #999999;
						font-size: 12px;
					}
				}
				.title {
					text-align: center;
					margin-top: 10px;
					white-space:nowrap; 
					overflow:hidden; 
					text-overflow:ellipsis;
					a {
						font-size:12px;
						color:#666666;
						&:hover{
							color:#ff3c23;
						}
					}
				}
			}
		}
	}
}
