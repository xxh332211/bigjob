@import '../../common/base';
@import '../../common/reset';
@import '../../component/head';
@import '../../component/foot';
@import '../../component/siderbar_right';
@import '../../common/swiper-3.4.2.min';
//版头底色
.swiper-slide {
	float: left;
}
.head_container {
	background: #fff;
}
//商品分类的一级分类始终显示
.nav {
	.nav_container {
		dt {
			.subnav {
				height:500px;
				ul {
					display: block;
				}
			}
		}
	}
}
//头部广告 -s
.headadv {
    background: #dc0100;
    width: 100%;
    height: 100px;
    .headnav_pic {
    	height:100%;
    	width:1200px;
		background:#666;
		position: relative;
		.icon_close {
			position: absolute;
			right:10px;
			top:0;
			cursor: pointer;
		}
		img {
			position: absolute;
			width: 100%;
			height: 100%;
			top:0;
			left:0;
		}
    }
}
//头部广告 -e

//banner 区域 -s
.banner {
	margin-bottom: 20px;
	.banner_holder {
		width:208px;
		height:500px;
	}
	.mainbanner {
		margin-left: 10px;
		margin-top: 10px;
		.swiper-container {
		    width:760px;
			height:360px;
			.swiper-button-white {
				background-color: #333;
				opacity: 0.3;
				width: 35px;
				height: 70px;
			}
			.swiper-pagination {
				text-align: right;
				.swiper-pagination-bullet {
				    width: 14px;
				    height: 14px;
				    display: inline-block;
				    border:1px solid #bdbdbd;
				    opacity: .2;
				    border-radius:0;
				    background: transparent;
				}
				.swiper-pagination-bullet-active {
					background: #ff7e18;
					opacity:.9;
				}
			}
		}
		.product {
			ul {
				margin-top: 10px;
				li {
					float: left;
					background:#ffffff;
					width:182px;
					height:120px;
					background: #666;
					margin-left: 11px;
					&:first-child {
						margin-left: 0;
					}
					img {
						width: 100%;
						height: 100%;
					}
				}
			}
		}
	}
	.user {
		background:#ffffff;
		box-shadow:0 1px 4px 1px #eeeeee;
		width:221px;
		height:490px;
		padding: 20px 15px;
		margin-top: 10px;
		.avatar {
			width:48px;
			height:48px;
			border-radius:24px;
		}
		.login {
			padding-left: 15px;
			p {
				margin-top: 5px;
				font-size:12px;
				color:#656565;
				a {
					font-size:12px;
					color:#656565;
				}
			}
		}
		.log_btn {
			margin-top: 14px;
			input{
				&:first-child {
					margin-right: 9px;
				}
			}
			.btn_in {
				background:#ff7e18;
				width:91px;
				height:32px;
				color: #fff;
				font-size: 14px;
				cursor:pointer;
				float: left;
			}
			.btn_sign {
				background:#ffffff;
				border:1px solid #ff7e18;
				width:91px;
				height:32px;
				cursor: pointer;
				font-size:14px;
				color:#ff7e18;
				float: left;
			}
		}
		.tab {
			width: 100%;
			div {
				float: left;
				width:50%;
				text-align: center;
				border-bottom:1px solid #e8e8e8;
				span {
					display: inline-block;
					padding: 10px;
					cursor: pointer;
					&.tab_selected {
						border-bottom: 1px solid #ff7e18;
					}
				}
			}
		}
		.news {
			width: 100%;
			padding-bottom: 20px;
			border-bottom: 1px dotted #e2e2e2;
			ul {
				li {
					margin-top: 18px;
					width: 100%;
					display:block;
					white-space:nowrap;
					overflow:hidden;
					text-overflow:ellipsis;
					a {
						font-size:12px;
						color:#666666;
						letter-spacing:0;
						&:hover {
							color : #ff7e18;
						}
					}
				}
			}
		}
		.icons {
			height: 65px;
			width: 100%;
			border-bottom: 1px dotted #e2e2e2;
			position: relative;
			ul {
				padding-left: 10px;
				li {
					float: left;
					// width: 24px;
					// height: 28px;
					margin-right: 19px;
					&:last-child {
						margin: 0;
					}
					img {
						width: 100%;
						height: 100%;
					}
				}
			}
		}
		.shouldknow {
			width: 100%;
			ul {
				li {
					height: 38px;
					line-height: 38px;
					&.border_dotted {
						height: 1px;
						border-bottom: 1px dotted #e2e2e2;
						width: 100%;
					}
					a {
						font-size:12px;
						color:#888888;
						&:hover {
							color : #ff7e18;
						}
					}
				}
			}
		}
	}
}
//banner 区域 -e
//advantage 区域 -s
.advantage {
	margin-bottom: 20px;
	.broadcast {
		background:#fff;
		height:36px;
		line-height: 36px;
		.icon_broadcast {
			margin-left: 21px;
		}
		.broadcast_logo {
			span {
				font-size:16px;
				color:#ff7e18;
				vertical-align: middle;
			}
		}
	}
	.activity_now {
		margin-top: 20px;
		ul {
			li {
				float: left;
				background:#00acff;
				border-radius:2px;
				width:290px;
				height:150px;
				margin-right: 13px;
				&:last-child {
					margin: 0;
				}
			}
		}
	}
	.brand {
		width: 100%;
		margin-top: 30px;
		.brand_title {
			width:245px;
			height:265px;
			background: #666;
		}
		.brand_list {
			ul {
				width:955px;
				border-bottom:1px solid #eaeaea;
				border-right:1px solid #eaeaea;
				box-sizing:border-box;
				li {
					float:left ;
					background:#ffffff;
					border-left:1px solid #eaeaea;
					border-top:1px solid #eaeaea;
					width:158px;
					height:88px;
					box-sizing:content-box;
					&:nth-of-type(7n){
						width:157px;
					}
					img {
						border:0;
						padding:0;
						float: left;
					}
				}
			}
		}
	}
}
