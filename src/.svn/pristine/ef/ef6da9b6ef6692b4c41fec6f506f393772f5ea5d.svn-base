//classify -s
.classify {
	margin-bottom: 30px;
	ul {
		li {
			background:#ffffff;
			border:1px solid #eeeeee;
			width:230px;
			height:230px;
			padding: 10px;
			float: left;
			margin-right: 11px;
			margin-bottom: 16px;
			&:first-child {
				margin-right: 17px;
			}
			&:nth-of-type(6n) {
				margin-right: 17px;
			}
			&:nth-of-type(5n) {
				margin-right: 0;
			}
			img {
				width:100%;
				height:100%;
				float: left;
			}
		}
	}
}
//classify -e
.adv {
	margin-bottom: 30px;
	height:200px;
	img {
		width:100%;
		height:100%;
	}
}

// floor -s
.floor {
	margin-bottom: 20px;
	.floor_main {
		.floor_title {
			position: relative;
			padding-bottom: 12px;
			margin-bottom: 8px;
			i{
				background:#ff6600;
				width:6px;
				height:20px;
				display: inline-block;
				float: left;
			}
			span {
				font-size:20px;
				color:#ff6600;
				letter-spacing:0;
				line-height: 1em;
				float: left;
				font-weight: bold;
				margin-left: 10px;
			}
			b {
				position: absolute;
				background:#ff3c23;
				border:1px solid #f9f9f9;
				border-radius:10px;
				width:58px;
				height:18px;
				text-align: center;
				font-size:12px;
				color:#ffffff;
				bottom:0;
				right: 5px;
				&:after {
					content:">";
					display: block;
					position: absolute;
					line-height: 1em;
					top: -2px;
					right:2px;
					font-size: 18px;
				}
			}
		}
		.floor_content {
			.content_l {
				width:280px;
				height:570px;
				padding-top:16px;
				position: relative;
				h3 {
					font-size:18px;
					color:#ffffff;
					text-indent: 26px;
				}
				h4 {
					font-size:14px;
					color:#ffffff;
					margin-top: 12px;
					text-indent: 26px;	
				}
				.pic {
					width: 160px;
					height:160px;
					margin:0 auto;
					margin-top: 20px;
				}
				.keywords {
					background:rgba(255,255,255,.2);
					width:280px;
					height:300px;
					position: absolute;
					bottom: 0;
					padding-left: 30px;
					padding-top: 20px;
					ul {
						li {
							background:rgba(255,255,255,.2);
							float:left;
							padding:10px 13px;
							margin-right: 10px;
							margin-bottom: 20px;
							a {
								font-size: 14px;
								color:#fff;
								&:hover {
									text-decoration: underline;
								}
							}
						}
					}
				}
			}
			.content_r {
				width: 920px;
				height: 570px;
				background: #fff;
				border-top: 1px solid #eee;
				ul {
					width: 100%;
					height: 50%;
					float: left;
					li {
						padding: 20px 20px 0 20px;
						height:286px;
						float: left;
						width:224px;
						border-bottom:1px solid #eee;
						border-right:1px solid #eee;
						&:first-child {
							padding-left: 41px;
							width:242px;
						}
						&:nth-of-type(5n) {
							padding-left: 41px;
							width:242px;
						}
						&:nth-of-type(4n) {
							padding-right:30px;
							width: 230px;
						}
						&:last-child {
							padding-right:30px;
							width: 230px;
						}						
						.pic {
							width: 180px;
							height: 180px;
							background: #666;
							img {
								width:100%;
								height:100%;
							}
						}
						.price {
							margin-top: 16px;
							line-height: 20px;
							span {
								font-size:20px;
								color:#ff3c23;
								letter-spacing:0;
								margin-right: 12px;
								float:left;
							}
							del {
								font-size:12px;
								color:#999999;
								float:left;
							}
						}
						.title {
							white-space:nowrap; 
							overflow:hidden; 
							text-overflow:ellipsis;
							font-size:12px;
							color:#666666;
							margin-top: 11px;
							a {
								color:#666666;
								&:hover {
									color: #ff3c23;
								}
							}
						}
					}
				}
			}
		}
	}
	.floor_adv {
		background:#e5e5e5;
		height:90px;
		margin-bottom: 23px;
		img {
			width : 100%;
			height:100%;
		}
	}
}
//floor -e

