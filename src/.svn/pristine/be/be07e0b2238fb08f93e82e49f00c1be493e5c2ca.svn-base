@charset "UTF-8";

.fl {
	float: left;
}

.fr {
	float: right;
}

.clearfix:after,
.clearfix:before {
	content: "";
	line-height: 0;
	height: 0;
	display: block;
	visibility: hidden;
	clear: both;
}

.banxin {
	width: 1200px;
	margin: 0 auto;
	border: none;
}

.middle {
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	z-index: 99;
}

.center {
	position: absolute;
	left: 50%;
	transform: translateX(-50%);
	z-index: 99;
}

.hide {
	display: none;
}

.show {
	display: block;
}

.scal {
	overflow: hidden;
	position: relative;
}

.scal img {
	width: 100%;
	height: 100%;
	transition: width .3s ease-out;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.scal:hover img {
	width: 110%;
	height: 110%;
	transition: width .3s ease-out;
}

.jump {
	position: relative;
	top: 0;
	transition: all .1s;
}

.jump:hover {
	top: -10px;
	transition: all .1s ease-out;
}

* {
	margin: 0;
	padding: 0;
	/*在移动端的特殊设置*/
	/*点击高亮效果*/
	-webkit-tap-highlight-color: transparent;
	/*透明其实就是不显示颜色*/
	/*设置所有盒子的宽度以边框开始计算*/
	-webkit-box-sizing: border-box;
	/*要兼容webkit浏览器内核厂商 这种情况一般是老的移动端浏览器*/
	box-sizing: border-box;
}

body {
	font-size: 14px;
	color: #333;
	background: #FAFAFA;
	font-family: sans-serif;
	/*默认字体*/
}

ul,
ol,
dl {
	list-style: none;
	padding: 0;
	margin: 0;
}

a {
	text-decoration: none;
	color: #666;
}

a:hover {
	color: #666;
}

i,
em,
b {
	font-style: normal;
	font-weight: normal;
	display: inline-block;
	line-height: 1em;
}

input,
textarea {
	border: none;
	outline: none;
	resize: none;
	/*特殊的属性定义 清楚浏览器给input自带的样式。*/
	-webkit-appearance: none;
	/*组件默认的样式空*/
}

h1,
h2,
h3,
h4,
h5,
h6 {
	font-weight: normal;
	line-height: 1em;
}

img {
	border: none;
}

