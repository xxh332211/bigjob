.nav {
	margin-top: 14px;
	border-bottom: 4px solid #ff7e18;
}

.nav .nav_container dt {
	float: left;
	background: #ff7e18;
	width: 200px;
	height: 40px;
	color: #fff;
	font-size: 16px;
	line-height: 38px;
	text-align: center;
	position: relative;
}

.nav .nav_container dt span {
	vertical-align: middle;
	margin-left: 14px;
}

.nav .nav_container dt .subnav {
	position: absolute;
	top: 44px;
	background: #ffffff;
	box-shadow: 0;
	width: 200px;
	height: 0;
	transition: height .3s;
	z-index: 999;
}

.nav .nav_container dt .subnav ul {
	width: 100%;
	padding: 16px 0 20px 20px;
	border-bottom: 1px dotted #e2e2e2;
	position: relative;
	box-sizing: border-box;
	display: none;
}

.nav .nav_container dt .subnav ul .cat_title {
	line-height: 1em;
	text-align: left;
}

.nav .nav_container dt .subnav ul .cat_title a {
	font-size: 14px;
	color: #666666;
	font-weight: bold;
}

.nav .nav_container dt .subnav ul:first-child:hover {
	background: #ff7e18;
}

.nav .nav_container dt .subnav ul:first-child:hover a {
	color: #fff;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat {
	display: block;
	opacity: 1;
	transition: width .3s;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat .first {
	color: #333;
	text-align: left;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat a {
	color: #333;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat a:hover {
	color: #ff7e18;
}

.nav .nav_container dt .subnav ul .second_cat {
	background: #ffffff;
	box-shadow: 0 1px 4px 1px #eeeeee;
	display: none;
	width: 1000px;
	height: 500px;
	z-index: 999;
	position: absolute;
	overflow-x: hidden;
	overflow-y: scroll;
	left: 200px;
	top: 0;
	transition: width .3s;
}

.nav .nav_container dt .subnav ul .second_cat:-webkit-scrollbar-track {
	border-radius: 10px;
	margin-right: 10px;
	background-color: #fff;
}

.nav .nav_container dt .subnav ul .second_cat:-webkit-scrollbar {
	width: 8px;
	background-color: #F5F5F5;
}

.nav .nav_container dt .subnav ul .second_cat:-webkit-scrollbar-thumb {
	border-radius: 10px;
	-webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
	background-color: #bdbdbd;
}

.nav .nav_container dt .subnav ul .second_cat:-webkit-scrollbar-track-piece {
	padding-left: 10px;
}

.nav .nav_container dt .subnav ul .second_cat ol {
	margin-left: 29px;
	margin-top: 30px;
	padding-right: 84px;
	width: 1000px;
}

.nav .nav_container dt .subnav ul .second_cat ol .first {
	color: #333;
	text-align: left;
	font-size: 14px;
	color: #333333;
	line-height: 14px;
	font-weight: bold;
}

.nav .nav_container dt .subnav ul .second_cat ol li {
	width: auto;
	margin-right: 20px;
}

.nav .nav_container dt .subnav ul .second_cat ol li a {
	color: #333;
	font-size: 12px;
	color: #333333;
}

.nav .nav_container dt .subnav ul li {
	float: left;
	margin-right: 20px;
	margin-top: 12px;
	line-height: 1em;
	text-align: left;
}

.nav .nav_container dt .subnav ul li a {
	font-size: 12px;
	color: #666666;
}

.nav .nav_container dt .subnav ul:last-child {
	border: none;
}

.nav .nav_container dt:hover .subnav {
	height: 500px;
	box-shadow: 0 1px 4px 1px #eeeeee;
	transition: height .3s;
}

.nav .nav_container dt:hover .subnav ul {
	display: block;
}

.nav .nav_container dd {
	float: left;
	height: 38px;
	line-height: 38px;
	margin-right: 15px;
	margin-left: 15px;
	position: relative;
}

.nav .nav_container dd .icon_nav {
	width: 16px;
	height: 16px;
	position: absolute;
	top: 5px;
}

.nav .nav_container dd a {
	font-size: 16px;
	color: #333333;
}

.nav .nav_container dd a:hover {
	color: #ff7e18;
}

.nav .nav_container dd a.sparate {
	color: #979797;
	font-weight: bold;
}

.nav .nav_container dd .activity {
	font-size: 16px;
	color: #ff7e18;
	font-weight: bold;
}

.nav .nav_container dd .activity:hover {
	color: #ff3c23;
}

