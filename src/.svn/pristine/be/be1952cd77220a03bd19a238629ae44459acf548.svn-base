.head_container {
	background: #fff
}
//头部 -s
.head {
    width: 100%;
    height: 30px;
    background:#f5f5f5;
	box-shadow:0 1px 0 0 #eeeeee;
    .head_content {
    	font-size:12px;
		line-height:12px;
		text-align:left;
		//头部左边
    	.head_content_l{
    		dt {
    			float: left;
    			height:100%;
    			line-height:30px;
    			.icon_phone {
    				vertical-align: -3px;
    				margin-right: 5px;
    			}
    		}
    		dd{
    			float: left;
    			height:100%;
    			line-height:30px;
    			margin-left: 20px;
    		}
    	}
		//头部右边
		.head_content_r {
			ul {
				height:30px;
				li {
					float: left;
					height: 30px;
					line-height: 30px;
					padding-left:12px;
					padding-right:12px;
					position: relative;
					.trangle_down {
						vertical-align: -3px;
					}
					.dropdown {
						position: absolute;
						width:100%;
						z-index: 998;
						left:0;
						overflow: hidden;
						display: none;
						background: #fff;
						ul {
							li {
								width:100%;
								float: left;
								a {
									font-size:10px;
									color:#666666;
									&:hover {
										color:#ff7e18
									}
								}
							}
						}
					}
					&:hover {
						background: #fff;
						a {
							color: #ff7e18;
						}
					}
				}
			}
		}
    }
}
//头部 -e

//logo区域 -s
.logo {
	margin-top: 14px;
	height: 80px;
	position: relative;
	.logo_pic {
		background: #666;
		width: 350px;
		height:80px;
	}
	.logo_search {
		margin-left: 48px;
		height: 80px;
		width: 492px;
		position: relative;
		.cover {
			.search {
				background:#ffffff;
				border:2px solid #ff7e18;
				width:388px;
				height:36px;
				box-sizing:content-box;
				color:#999;
				font-size: 12px;
				text-indent: 15px;
			}
			.btn {
				background:#ff7e18;
				width:100px;
				height:40px;
				border: 0;
				cursor:pointer;
				color:#fff;
				font-size:14px;
			}
			.notice {
				position: absolute;
				bottom: -20px;
				dd {
					float: left;
					margin-right: 5px;
					a{
						font-size:12px;
						color:#565656;
					}
				}
			}
		}
	}
	.logo_right{
		height: 80px;
		width: 280px;
		position: relative;
		.cover{
			.logo_cube {
				margin-left: 10px;
				background:#ffffff;
				border:1px solid #e8e8e8;
				width:128px;
				height:38px;
				font-size:14px;
				color:#666666;
				text-align:center;
				line-height: 38px;
				position: relative;
				cursor: pointer;
				.coverline {
					width:100%;
					height:5px;
					position: absolute;
					z-index: 999;
					bottom: -3px;
					right:0;
					left:0;
					background: #fff;
					display: none;
				}
				.bigdrop {
					position: absolute;
					width: 310px;
					overflow: hidden;
					background: #fff;
					border:0;
					left: -183px;
					top: 37px;
					z-index: 99;
					border:1px solid #e8e8e8;
					display: none;
					.has_cargo {
						max-height: 457px - 38px;
						p{
							font-size:12px;
							height:24px;
							color:#666666;
							line-height:24px;
							text-align:left;
							text-indent: 9px;
							position: absolute;
						    top: 0;
						    left: 0;
							background: #fff;
							width: 100% ;
						}
						.pannel {
							width: 310px;
							height:44px;
							background: #f5f5f5;
							position:absolute;
							bottom: 0;
							line-height: 44px;
							text-align: left;
							span {
								// float: left;
								font-size:12px;
								color:#666666;
								line-height: 1em;
								margin-left: 10px;
								b {
									background: transparent;
									border-radius: 0;
									color: #ff3c23;
									font-weight: bold;
									width:auto;
									line-height: 1em;
								}
							}
							input {
								background:#ff7e18;
								border-radius:4px;
								width:80px;
								height:30px;
								color:#fff;
								float: right;
								margin-top:7px;
								margin-right: 13px;
								cursor: pointer;
							}
						}
						ul {
							overflow-y:auto;
							max-height: 457px - 38px;
							padding-top: 24px;
							padding-bottom: 44px;
							li {
								width:100%;
								height: 68px;
								padding-left: 9px;
								padding-top: 7px;
								background: #fff;
								float: left;
								&:hover {
									background: #f5f5f5;
								}
								.pic {
									width: 52px;
									height: 52px;
									img {
										width: 100%;
										height:100%;
									}
								}
								.product {
									.product_l {
										width: 132px;
										line-height: 1em;
										text-align: left;
										font-size:12px;
										color:#888888;
										margin-left: 6px;
										.title {
											display:block;
											white-space:nowrap; 
											overflow:hidden; 
											text-overflow:ellipsis;
											width: 100%;
											margin-bottom:6px;
										}
										.sku {
											width: 100%;
											margin-bottom: 6px;
										}
										.type {
											width: 100%;
											margin-bottom: 6px;
										}
									}
									.product_r {
										width: 86px;
										line-height: 1em;
										text-align: left;
										font-size:12px;
										color:#888888;
										text-align: right;
										.price {
											width: 100%;
											margin-bottom: 6px;
										}
										.del {
											width: 100%;
											a {
												@extend .product_r;
												&:hover {
													color: #ff3c23;
												}
											}
										}
									}
								}
							}
						}
					}
					.no_cargo {
						width: 100%;
						height: 70px;
						padding-left: 39px;
						line-height: 70px;
						i {
							float: left;
							margin-top: 10px;
						}
						p {
							float: left;
							margin-left: 18px;
							font-family:'NotoSansHans-Regular';
							font-size:12px;
							color:#999999;
						}
					}
				}
				&:hover {
					color: #ff7e18;
					.bigdrop {
						min-height: 70px;
						max-height: 457px - 38px;
						border:1px solid #e8e8e8;
						display: block;
						.no_cargo {
							i {
								float: left;
							}
							p {
								float: left;
								color:#999999;
							}
						}
					}
					.coverline {
						display: block;
					}
				}
				span {
					vertical-align: middle;
				}
				b {
					background:#ff3011;
					width:18px;
					height:18px;
					border-radius:100%;
					color: #fff;
					vertical-align: middle;
					line-height: 18px;
					font-size: 10px;
				}
			}
		}
	}
	
}
//logo区域 -e

//nav区域 -s
.nav {
	margin-top: 14px;
	border-bottom: 4px solid #ff7e18;
	.nav_container{
		dt {
			float: left;
			background:#ff7e18;
			width:200px;
			height:40px;
			color:#fff;
			font-size: 16px;
			line-height: 38px;
			text-align: center;
			position: relative;
			span {
				vertical-align: middle;
				margin-left: 14px;
			}
			.subnav {
				position: absolute;
				top:44px;
				background:#ffffff;
				box-shadow:0;
				width:200px;
				height:0;
				transition: height .3s;
				z-index: 999;
				// overflow: hidden;
				ul {
					width:100%;
					padding : 16px 0 20px 20px;
					border-bottom: 1px dotted #e2e2e2;
					position: relative;
					box-sizing:border-box;
					display: none;
					.cat_title {
						line-height: 1em;
						text-align: left;
						a {
							
							font-size:14px;
							color:#666666;
							font-weight: bold;
						}
					}
					&:first-child {
						&:hover {
							background: #ff7e18;
							a {
								color: #fff;
							}
							.second_cat {
								display: block;
								opacity: 1;
								transition: width .3s;
								.first {
									color: #333;
									text-align: left;
								}
								a {
									color: #333;
									&:hover {
										color: #ff7e18;
									}
								}
							}
						}
					}
					.second_cat {
						background:#ffffff;
						box-shadow:0 1px 4px 1px #eeeeee;
						display: none;
						width: 1000px;
						height:500px;
						z-index: 999;
						position: absolute;
						overflow-x: hidden;
						overflow-y: scroll;
						left: 200px;
						top: 0;
						transition: width .3s;
						&:-webkit-scrollbar-track {
						    // -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
						    border-radius: 10px;
						    margin-right: 10px;
						    background-color: #fff;
						}
						&:-webkit-scrollbar {
						    width: 8px;
						    background-color: #F5F5F5;
						}

						&:-webkit-scrollbar-thumb {
						    border-radius: 10px;
						    -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, .3);
						    background-color: #bdbdbd;
						}
						&:-webkit-scrollbar-track-piece{
							padding-left:10px;
						}
						ol {
							margin-left: 29px;
							margin-top: 30px;
							padding-right: 84px;
							width: 1000px;
							.first {
								color: #333;
								text-align: left;
								font-size:14px;
								color:#333333;
								line-height:14px;
								font-weight: bold;
							}
							li {
								width: auto;
								margin-right: 20px;
								a {
									color:#333;
									
									font-size:12px;
									color:#333333;
								}
							}
						}
					}
					li {
						float: left;
						margin-right: 20px;
						margin-top: 12px;
						line-height: 1em;
						text-align: left;
						height:1em;
						a {
							
							font-size:12px;
							color:#666666;
						}
					}
					&:last-child {
						border:none;
					}
				}
			}
			&:hover {
				.subnav {
					height: 500px;
					box-shadow:0 1px 4px 1px #eeeeee;
					transition: height .3s;
					ul {
						display: block;
					}
				}
			}
		}
		dd {
			float: left;
			height: 38px;
			line-height: 38px;
			margin-right:15px;
			margin-left: 15px;
			position:relative;
			.icon_nav {
				width:16px;
				height:16px;
				position: absolute;
				top: 5px;
			}
			a {
				
				font-size:16px;
				color:#333333;
				&:hover {
					color:#ff7e18;
				}
				&.sparate {
					color:#979797;
					font-weight: bold;
				}
			}
			.activity {
				
				font-size:16px;
				color:#ff7e18;
				font-weight: bold;
				&:hover {
					color:#ff3c23;
				}
			}
		}
	}	
}
//nav区域 -e