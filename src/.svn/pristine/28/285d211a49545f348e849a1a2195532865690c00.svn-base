//头部 -s
.head {
    width: 100%;
    height: 30px;
    background:#f5f5f5;
	box-shadow:0 1px 0 0 #eeeeee;
    .head_content {
    	font-size:12px;
		line-height:12px;
		text-align:left;
		//头部左边
    	.head_content_l {
    		dt {
    			float: left;
    			height:100%;
    			line-height:30px;
    			.icon_phone {
    				vertical-align: -3px;
    				margin-right: 5px;
    			}
    		}
    		dd {
    			float: left;
    			height:100%;
    			line-height:30px;
    			margin-left: 20px;
    		}
    	}
		//头部右边
		.head_content_r {
			ul {
				height:30px;
				li {
					float: left;
					height: 30px;
					line-height: 30px;
					padding-left:12px;
					padding-right:12px;
					position: relative;
					.trangle_down {
						vertical-align: -3px;
					}
					.dropdown {
						position: absolute;
						width:100%;
						z-index: 998;
						left:0;
						overflow: hidden;
						display: none;
						background: #fff;
						ul {
							li {
								width:100%;
								float: left;
								a {
									font-size:10px;
									color:#666666;
									&:hover {
										color:#ff7e18
									}
								}
							}
						}
					}
					&:hover {
						background: #fff;
						a {
							color: #ff7e18;
						}
					}
				}
			}
		}
    }
}
//头部 -e

//user_logo -s
.user_logo {
	height:60px;
	background: #ff7e18;
	.user_logo_center {
		height: 60px;
		.user_logo_l {
			display: table;
			height:60px;
			.user_title {
				display: table-cell;
				font-size:26px;
				color:#ffffff;
				line-height:26px;
				text-align:left;
				font-size: bold;
				vertical-align: middle;
			}
			.user_goindex {
				border:1px solid #ffffff;
				display: inline-block;
				padding:10px;
				color: #fff;
				vertical-align: middle;
				margin-top: 10px;
				margin-left: 50px;
				cursor: pointer;
			}
		}
		.user_logo_r {
			height: 60px;
			.logo_cube {
				display: inline-block;
				vertical-align: middle;
				margin-left: 10px;
				background:#ffffff;
				border:1px solid #e8e8e8;
				width:128px;
				height:35px;
				font-size:14px;
				color:#666666;
				text-align:center;
				line-height: 32px;
				position: relative;
				cursor: pointer;
				.coverline {
					width:100%;
					height:5px;
					position: absolute;
					z-index: 999;
					bottom: -5px;
					right:0;
					left:0;
					background: #fff;
					display: none;
				}
				.bigdrop {
					position: absolute;
					width: 310px;
					overflow: hidden;
					background: #fff;
					border:0;
					left: -183px;
					top: 34px;
					z-index: 99;
					border:1px solid #e8e8e8;
					display: none;
					.has_cargo {
						max-height: 457px - 38px;
						p{
							font-size:12px;
							height:24px;
							color:#666666;
							line-height:24px;
							text-align:left;
							text-indent: 9px;
							position: absolute;
						    top: 0;
						    left: 0;
							background: #fff;
							width: 100% ;
						}
						.pannel {
							width: 310px;
							height:44px;
							background: #f5f5f5;
							position:absolute;
							bottom: 0;
							line-height: 44px;
							text-align: left;
							span {
								// float: left;
								font-size:12px;
								color:#666666;
								line-height: 1em;
								margin-left: 10px;
								b {
									background: transparent;
									border-radius: 0;
									color: #ff3c23;
									font-weight: bold;
									width:auto;
									line-height: 1em;
								}
							}
							input {
								background:#ff7e18;
								border-radius:4px;
								width:80px;
								height:30px;
								color:#fff;
								float: right;
								margin-top:7px;
								margin-right: 13px;
								cursor: pointer;
							}
						}
						ul {
							overflow-y:auto;
							max-height: 457px - 38px;
							padding-top: 24px;
							padding-bottom: 44px;
							li {
								width:100%;
								height: 68px;
								padding-left: 9px;
								padding-top: 7px;
								background: #fff;
								float: left;
								&:hover {
									background: #f5f5f5;
								}
								.pic {
									width: 52px;
									height: 52px;
									img {
										width: 100%;
										height:100%;
									}
								}
								.product {
									.product_l {
										width: 132px;
										line-height: 1em;
										text-align: left;
										font-size:12px;
										color:#888888;
										margin-left: 6px;
										.title {
											display:block;
											white-space:nowrap; 
											overflow:hidden; 
											text-overflow:ellipsis;
											width: 100%;
											margin-bottom:6px;
										}
										.sku {
											width: 100%;
											margin-bottom: 6px;
										}
										.type {
											width: 100%;
											margin-bottom: 6px;
										}
									}
									.product_r {
										width: 86px;
										line-height: 1em;
										text-align: left;
										font-size:12px;
										color:#888888;
										text-align: right;
										.price {
											width: 100%;
											margin-bottom: 6px;
										}
										.del {
											width: 100%;
											a {
												@extend .product_r;
												&:hover {
													color: #ff3c23;
												}
											}
										}
									}
								}
							}
						}
					}
					.no_cargo {
						width: 100%;
						height: 70px;
						padding-left: 39px;
						line-height: 70px;
						i {
							float: left;
							margin-top: 10px;
						}
						p {
							float: left;
							margin-left: 18px;
							font-family:'NotoSansHans-Regular';
							font-size:12px;
							color:#999999;
						}
					}
				}
				&:hover {
					color: #ff7e18;
					.bigdrop {
						min-height: 70px;
						max-height: 457px - 38px;
						border:1px solid #e8e8e8;
						display: block;
						.no_cargo {
							i {
								float: left;
							}
							p {
								float: left;
								color:#999999;
							}
						}
					}
					.coverline {
						display: block;
					}
				}
				span {
					vertical-align: middle;
				}
				b {
					background:#ff3011;
					width:18px;
					height:18px;
					border-radius:100%;
					color: #fff;
					vertical-align: middle;
					line-height: 18px;
					font-size: 10px;
				}
			}
			.search {
				display: inline-block;
				height: 60px;
				vertical-align: middle;
				line-height: 60px;
				span {
					display: inline-block;
					width: 56px;
					height:34px;
					background: #f7f7f7;
					font-size:12px;
					color:#666666;
					line-height:34px;
					text-align:center;
					vertical-align: middle;
					cursor: pointer;
				}
				input {
					height: 34px;
					width: 185px-56px;
					vertical-align: middle;
					line-height: 34px;
					font-size:12px;
					color:#999999;
					text-indent: 1em;
				}

			}
		}
	}
}
