	.user_content_l {
		width: 200px;
		background: #fff;
		dl {
			padding-top: 20px;
			overflow: hidden;
			border-bottom: 1px solid #eee;
			dt {
				margin-left:60px;
				margin-bottom: 15px;
				position: relative;
				&:before {
					content: "";
