//modal_adress -s
.modal_adress {
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: 998;
	top: 0;
	left: 0;
	.cover {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		background: black;
		opacity: .5;
		z-index: 997;
	}
	&>.content {
		background: #ffffff;
		box-shadow: 0 0 3px 0 #deaf0e;
		border-radius: 6px;
		width: 510px;
		height: 410px;
		position: absolute;
		z-index: 999;
		top: 50%;
		left: 50%;
		margin-top: -205px;
		margin-left: -255px;
		padding-top: 20px;
		padding-left: 20px;
		.close {
			position: absolute;
			right: 20px;
			top: 20px;
			cursor: pointer;
		}
		.title {
			margin-bottom: 20px;
			font-size: 20px;
			color: #ff7e18;
			line-height: 20px;
			text-align: left;
		}
		.row {
			height: 35px;
			line-height: 35px;
			margin-bottom: 5px;
			&.large {
				height: 60px;
			}
			label {
				margin-right: 20px;
				cursor: pointer;
				i {
					margin-right: 3px;
				}
				&.selected {
					.radio {
						background: #3998fc;
					}
				}
			}
			span {
				float: left;
				width: 70px;
				font-size: 12px;
				color: #666666;
			}
			b {
				font-size: 12px;
				color: #333333;
			}
			input {
				background: #ffffff;
				border: 1px solid #e5e5e5;
				width: 138px;
				height: 28px;
				text-indent: 25px;
				font-size: 12px;
				color: #666666;
				&.short {
					width: 80px;
				}
			}
			textarea {
				background: #f9f9f9;
				border: 1px solid #eeeeee;
				width: 338px;
				height: 58px;
				font-size: 12px;
				color: #666666;
				text-indent: 15px;
				padding: 10px;
			}
			button {
				background-image: linear-gradient(-180deg, #fe9a02 0%, #f78400 100%);
				border-radius: 2px;
				width: 120px;
				height: 38px;
				border: none;
				font-size: 16px;
				color: #ffffff;
				outline: none;
				margin-left: 70px;
				cursor: pointer;
			}
			.cheackbox {
				margin-left: 110px;
				height: 35px;
				padding-top: 10px;
				&.checked {
					.box {
						.iconfont {
							display: block;
						}
					}
				}
				.box {
					background: #ffffff;
					border: 1px solid #eeeeee;
					width: 14px;
					height: 14px;
					position: relative;
					cursor: pointer;
					.iconfont {
						position: absolute;
						top: -6px;
						left: -6px;
						width: 14px;
						height: 14px;
						font-size: 25px;
						display: none;
					}
				}
				.content {
					font-size: 12px;
					color: #666666;
					line-height: 12px;
					margin-left: 10px;
					user-select: none;
					cursor: pointer;
				}
			}
			.select {
				width: 80px;
				height: 30px;
				outline: none;
				border: 1px solid #e5e5e5;
				float: left;
				position: relative;
				margin-left: 10px;
				&:first-child {
					margin-left: 0;
				}
				&>.iconfont {
					width: 14px;
					height: 14px;
					position: absolute;
					top: 50%;
					right: 5px;
					font-size: 14px;
					margin-top: -3px;
					color: #7c7c7c;
				}
				.select_head {
					width: 78px;
					height: 28px;
					padding-left: 20px;
					line-height: 33px;
					font-size: 12px;
					color: #333333;
					cursor: pointer;
					background: #fff;
				}
				.select_dropdown {
					background: #fff;
					position: absolute;
					left: -1px;
					top: 29px;
					width: 80px;
					display: none;
					z-index: 999;
					border: {
						left: 1px solid #e5e5e5;
						bottom: 1px solid #e5e5e5;
						right: 1px solid #e5e5e5;
					}
					ul {
						li {
							padding-left: 25px;
							height: 30px;
							color: #333333;
							font-size: 12px;
							position: relative;
							line-height: 30px;
							cursor: pointer;
							&:hover {
								background: #f6f6f6;
								.iconfont {
									color: #f6f6f6;
								}
							}
							.iconfont {
								width: 12px;
								height: 12px;
								font-size: 15px;
								background: #ff3c23;
								color: #fff;
								position: absolute;
								left: 7px;
								top: 50%;
								margin-top: -6px;
								display: none;
							}
							&.selected {
								color: #ff3c23;
								.iconfont {
									display: block;
								}
							}
						}
					}
				}
			}
		}
	}
}

