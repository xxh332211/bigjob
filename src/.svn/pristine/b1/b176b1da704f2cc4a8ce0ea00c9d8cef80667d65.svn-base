<div class="modal_cover hide">
	<div class="modal_cotainer">
	{{each array as value i}}
		{{if i == 0}}
		<div class="closes">
			<i class="iconfont">&#xe619;</i>
		</div>
		{{/if}}
		
		{{if value.type == "title"}}
		<div class="title">{{value.content}}</div>
		{{/if}}

		{{if value.type == "alert"}}
		<div class="alert">
			{{value.content}}
		</div>
		{{/if}}

		{{if value.type == "note"}}
		<div class="paragraph">
			{{value.content}}
		</div>
		{{/if}}

		{{if value.type =="input"}}
		<div class="input" data-name={{value.name}}>
			<label>
				<span>
				{{if value.require}}
				<b>*</b>
				{{/if}}
				{{value.text}}
				</span>:
				<input type="text" placeholder={{value.holder}} data-child="{{value.type}}" data-cheack="{{value.cheack}}" class="{{value.require}}">
				<div class="nomatch" style="display: none;">
					<i class="iconfont">&#xe646;</i>
					{{value.tip}}
				</div>
			</label>
		</div>
		{{/if}}

		{{if value.type == "identify"}}
		<div class="input identify" data-name={{value.name}}>
			<label>
				<span>{{value.text}}</span>:
				<input type="text" placeholder={{value.holder}} data-child="{{value.type}}" data-cheack="{{value.cheack}}" class={{value.require}}>
				<div class="nomatch" style="display: none;">
					<i class="iconfont">&#xe646;</i>
					{{value.tip}}
				</div>
				<img src="..." alt="">
			</label>
		</div>
		{{/if}}

		{{if value.type =="date"}}
		<div class="input" data-name={{value.name}}>
			<label>
				<span>
				{{if value.require}}
				<b>*</b>
				{{/if}}
				{{value.text}}
				</span>:
				<input type="text" placeholder={{value.holder}} data-child="{{value.type}}" onClick="laydate({istime: true, format: 'YYYY-MM-DD hh:mm:ss'})" class="laydate-icon {{value.require}}" data-cheack={{value.cheack}}>
				<div class="nomatch" style="display: none;">
					<i class="iconfont">&#xe646;</i>
					{{value.tip}}
				</div>
			</label>
		</div>
		{{/if}}

		{{if value.type == "fullinput"}}
		<div class="fullinput">
			<div data-name={{value.name}}>
				{{if value.sub!="password"}}
				<input type="text" placeholder={{value.holder}} data-child={{value.type}} data-cheack="{{value.cheack}}" class={{value.require}}>
				<div class="nomatch" style="display: none;">
					<i class="iconfont">&#xe646;</i>
					{{value.tip}}
				</div>
				{{/if}}
				{{if value.sub == "password"}}
				<input type="password" placeholder={{value.holder}} data-child={{value.type}} data-cheack="{{value.cheack}}" class={{value.require}}>
				<div class="nomatch" style="display: none;">
					<i class="iconfont">&#xe646;</i>
					{{value.tip}}
				</div>
				{{/if}}
				<i class="iconfont">{{value.icon}}</i>
				{{if value.sub == "identify"}}
				<span>
					<img src="..." alt="">
				</span>
				{{/if}}
				{{if value.sub == "getcode"}}
				<span class="getcode">
					获取验证码
				</span>
				{{/if}}
			</div>
		</div>
		{{/if}}
		
		{{if value.type == "option"}}
		<div class="option">
			<div class="btn clearfix">
				{{if value.sub.indexOf('cancel')>-1}}
				<div class="cancel">取消</div>
				{{/if}}

				{{if value.sub.indexOf('sure')>-1}}
				<div class="sure">{{value.text}}</div>
				{{/if}}

				{{if value.sub.indexOf('big')>-1}}
				<div class="{{value.subclass}} big">{{value.text}}</div>
				{{/if}}
				{{if value.sub.indexOf('login')>-1}}
				<div class="login_tag clearfix">
					<a href={{value.link1}}>
						<span>忘记登录密码?</span>
					</a>
					<a href={{value.link2}}>
						<span>免费注册</span>
					</a>
				</div>
				{{/if}}
			</div>
		</div>
		{{/if}}

		{{if value.type == "protocol"}}
		<div class="protocol">
			{{#value.content}}
		</div>
		{{/if}}
		
		{{if value.type == "checkbox"}}
		<div class="checkbox clearfix">
			<i class="iconfont nor">&#xe640;</i>
			{{if value.classname}}
			<span class="{{value.classname}}"><b>我已阅读并同意</b>{{#value.content}}</span>
			{{/if}}
			{{if !value.classname}}
			<span><b>{{value.content}}</b></span>
			{{/if}}
		</div>
		{{/if}}
	{{/each}}
	</div>
</div>