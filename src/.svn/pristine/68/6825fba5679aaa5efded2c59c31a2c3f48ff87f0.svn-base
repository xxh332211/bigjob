@import '../../common/reset';
@import '../../common/base';
@import '../../component/head.scss';
body {
    background: #fff;
}

.taobao_index {
    &>.taobao_head {
        height: 40px;
        background: #333333;
        .thead_center {
            height: 40px;
            background: #333333;
            .logistics {
                height: 40px;
                color: #fff;
                padding-left: 20px;
                padding-right: 20px;
                line-height: 40px;
                cursor: pointer;
                vertical-align: text-bottom;
                &:hover {
                    background: rgba(255, 255, 255, 0.2)
                }
                .iconfont {
                    font-size: 18px;
                    height: 18px;
                    width: 24px;
                    color: #fff;
                    font-size: 24px;
                }
            }
            .order {
                height: 40px;
                color: #fff;
                padding-left: 20px;
                padding-right: 20px;
                line-height: 40px;
                cursor: pointer;
                &:hover {
                    background: rgba(255, 255, 255, 0.2)
                }
                .iconfont {
                    width: 17px;
                    height: 20px;
                    font-size: 17px;
                    color: #fff;
                    font-size: 24px;
                    vertical-align: text-bottom;
                }
            }
        }
    }
    &>.taobao_logo {
        height: 91px;
        line-height: 91px;
        &>.taobao_logopic {
            height: 91px;
            line-height: 91px;
            margin-right: 50px;
            img {
                vertical-align: middle;
            }
        }
        &>.taobao_nav {
            li {
                float: left;
                margin-right: 30px;
                a {
                    font-size: 16px;
                    color: #333333;
                    &:hover {
                        font-size: 16px;
                        color: #ff5001;
                    }
                }
            }
        }
        &>.taobao_search {
            &>input {
                background: #ffffff;
                border: 1px solid #e0e0e0;
                width: 294px;
                height: 50px;
                text-indent: 13px;
                &:focus {
                    border-color: #ff7e18;
                    &+button {
                        background: #ff7e18;
                        border-color: #ff7e18;
                        color: #fff;
                    }
                }
            }
            &>button {
                background: #ffffff;
                border: 1px solid #e0e0e0;
                border-radius: 2px;
                width: 50px;
                height: 50px;
                text-align: center;
                line-height: 47px;
                color: #616161;
                outline: none;
                cursor: pointer;
                &:active {
                    background: #ff7e18;
                    border-color: #ff7e18;
                    color: #fff;
                }
                .iconfont {
                    font-size: 25px;
                    margin-right: 0;
                    vertical-align: middle;
                }
            }
        }
    }
    &>.taobao_banner {
        height: 460px;
        background: pink;
        margin-top: 10px;
        .banner_nav {
            background: #5C5657;
            width: 235px;
            height: 460px;
            ul {
                width: 100%;
                height: 100%;
                position: relative;
                li {
                    width: 235px;
                    height: 42px;
                    padding-left: 38px;
                    padding-top: 10px;
                    padding-right: 35px;
                    color: #fff;
                    font-size: 16px;
                    color: #ffffff;
                    cursor: pointer;
                    &>span {
                        float: left;
                    }
                    .iconfont {
                        vertical-align: middle;
                        margin-right: 0;
                        line-height: 27px;
                        font-size: 16px;
                    }
                    .tips_box {
                        width: 680px;
                        height: 460px;
                        background: #fff;
                        font-size: 14px;
                        color: #333333;
                        position: absolute;
                        overflow: auto;
                        left: 235px;
                        top: 0;
                        display: none;
                        z-index: 999;
                        border: 1px solid #e5e5e5;
                        dl {
                            width: 100%;
                            padding-top: 37px;
                            padding-bottom: 37px;
                            padding-left: 50px;
                            padding-right: 90px;
                            dd {
                                float: left;
                                margin-right: 50px;
                                margin-bottom: 50px;
                                a {
                                    font-size: 14px;
                                    color: #333333;
                                    &:hover {
                                        color: #ff7e18;
                                    }
                                }
                            }
                        }
                    }
                    &:hover {
                        background: #ff7e18;
                        .tips_box {
                            display: block;
                        }
                    }
                }
            }
        }
        .slider_box {
            width: 965px;
            height: 460px;
            position: relative;
            overflow: hidden;
            .go_left {
                position: absolute;
                left: 0;
                top: 170px;
                cursor: pointer;
                z-index: 998;
            }
            .go_right {
                position: absolute;
                right: 0;
                top: 170px;
                cursor: pointer;
                z-index: 998;
            }
            .slide_nav {
                position: absolute;
                bottom: 17px;
                right: 70px;
                z-index: 999;
                ul {
                    li {
                        background: #91918f;
                        border: 1px solid #b4b4b3;
                        width: 10px;
                        height: 10px;
                        border-radius: 100%;
                        float: left;
                        margin-right: 22px;
                        cursor: pointer;
                        &.active {
                            background: #fff;
                        }
                    }
                }
            }
            .slides {
                width: 10000px;
                position: absolute;
                left: 0;
                top: 0;
                .slide {
                    width: 965px;
                    height: 460px;
                    float: left;
                }
            }
        }
    }
    &>.taobao_choice {
        margin-top: 41px;
        .choice_title {
            height: 146px;
            .title_head {
                height: 146px;
                width: 102px;
                background: #ff7e18;
                text-align: center;
                padding-top: 30px;
                span {
                    display: block;
                    font-size: 20px;
                    color: #ffffff;
                }
            }
            &>ul {
                li {
                    float: left;
                    width: 137px;
                    height: 146px;
                    text-align: center;
                    padding-top: 33px;
                    cursor: pointer;
                    background: #fff;
                    &.active {
                        background: #f5f5f5;
                    }
                    span {
                        display: block;
                        font-size: 20px;
                        color: #666666;
                    }
                }
            }
        }
        .products {
            margin-top: 20px;
            overflow: hidden;
            height: 680px;
            position: relative;
            .main_box {
                width: 100000px;
                position: absolute;
                left: 0;
                top: 0;
                .box {
                    width: 1200px;
                    float: left;
                    .single {
                        width: 230px;
                        height: 320px;
                        border: 1px solid #eee;
                        margin-bottom: 20px;
                        float: left;
                        margin-right: 12px;
                        &:nth-child(5n) {
                            margin-right: 0;
                        }
                        .pic {
                            width: 228px;
                            height: 228px;
                            background: pink;
                            position: relative;
                            overflow: hidden;
                            &:hover {
                                .updata {
                                    bottom: 0;
                                    transition: all .3s;
                                }
                            }
                            .updata {
                                position: absolute;
                                bottom: -100%;
                                left: 0;
                                width: 228px;
                                height: 30px;
                                overflow: hidden;
                                transition: all .3s;
                                span {
                                    display: block;
                                    float: left;
                                    width: 114px;
                                    text-align: center;
                                    line-height: 30px;
                                    background: #fe892e;
                                    font-size: 14px;
                                    color: #ffffff;
                                    cursor: pointer;
                                    &:hover {
                                        background: #ff7e18;
                                    }
                                }
                            }
                        }
                        .title {
                            overflow: hidden;
                            text-overflow: ellipsis;
                            white-space: nowrap;
                            padding: 15px;
                            a {
                                font-size: 14px;
                                color: #333333;
                                &:hover {
                                    color: #ff7e18;
                                }
                            }
                        }
                        .price {
                            font-size: 20px;
                            color: #ff7e18;
                            padding-left: 15px;
                            span {
                                font-size: 18px;
                            }
                        }
                    }
                }
            }
        }
    }
    &>.taobao_stars {
        margin-top: 40px;
        position: relative;
        .star_title {
            font-size: 20px;
            color: #333333;
            margin-bottom: 15px;
        }
        .control {
            position: absolute;
            right: 0;
            top: 0;
            .left {
                background: #ffffff;
                border: 1px solid #e0e0e0;
                width: 36px;
                height: 24px;
                text-align: center;
                line-height: 24px;
                cursor: pointer;
                user-select: none;
                float: left;
                &:active {
                    .iconfont {
                        color: #ff5001
                    }
                }
                .iconfont {
                    font-size: 16px;
                    margin-right: 0;
                    color: #b0b0b0;
                }
            }
            .right {
                background: #ffffff;
                border: 1px solid #e0e0e0;
                width: 36px;
                height: 24px;
                cursor: pointer;
                text-align: center;
                line-height: 24px;
                user-select: none;
                float: right;
                &:active {
                    .iconfont {
                        color: #ff5001
                    }
                }
                .iconfont {
                    font-size: 16px;
                    margin-right: 0;
                    color: #b0b0b0;
                }
            }
        }
        .star_product {
            width: 1200px;
            overflow: hidden;
            height: 323px;
            position: relative;
            .slides {
                width: 10000px;
                height: 323px;
                position: absolute;
                left: 0;
                top: 0;
                .slide {
                    width: 1200px;
                    height: 323px;
                    float: left;
                    .single {
                        height: 323px;
                        width: 230px;
                        border: 1px solid #eee;
                        float: left;
                        margin-right: 12px;
                        &:nth-child(5n) {
                            margin-right: 0;
                        }
                        .color_wrap {
                            height: 3px;
                            width: 228px;
                        }
                        .pic {
                            width: 228px;
                            height: 228px;
                            background: pink;
                            position: relative;
                            overflow: hidden;
                            &:hover {
                                .updata {
                                    bottom: 0;
                                    transition: all .3s;
                                }
                            }
                            .updata {
                                position: absolute;
                                bottom: -100%;
                                left: 0;
                                width: 228px;
                                height: 30px;
                                overflow: hidden;
                                transition: all .3s;
                                span {
                                    display: block;
                                    float: left;
                                    width: 114px;
                                    text-align: center;
                                    line-height: 30px;
                                    background: #fe892e;
                                    font-size: 14px;
                                    color: #ffffff;
                                    cursor: pointer;
                                    &:hover {
                                        background: #ff7e18;
                                    }
                                }
                            }
                        }
                        .title {
                            overflow: hidden;
                            text-overflow: ellipsis;
                            white-space: nowrap;
                            padding: 15px;
                            a {
                                font-size: 14px;
                                color: #333333;
                                &:hover {
                                    color: #ff7e18;
                                }
                            }
                        }
                        .price {
                            font-size: 20px;
                            color: #ff7e18;
                            padding-left: 15px;
                            span {
                                font-size: 18px;
                            }
                        }
                    }
                }
            }
        }
    }
    &>.gray_container {
        width: 100%;
        .taobao_mainproduct {
            .title {}
            .control {}
            .products {
                height: 652px;
                .big_pic {}
                .product_box {}
            }
        }
    }
}