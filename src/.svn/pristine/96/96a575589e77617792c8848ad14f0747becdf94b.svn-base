body {
	width: 100%;
	height: 100%;
}

html {
	width: 100%;
	height: 100%;
}

.redpack {
	width: 100%;
	height: 100%;
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 999;
}

.redpack .cover {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #000;
	filter: Alpha(opacity=7);
	-moz-opacity: 0.7;
	opacity: 0.7;
	z-index: 998;
}

.redpack .center {
	width: 374px;
	height: 481px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -240px;
	margin-left: 0;
	z-index: 999;
}

.redpack .center .red_price {
	position: absolute;
	top: 290px;
	left: 135px;
	font-size: 50px;
	font-weight: bold;
	line-height: 1em;
	letter-spacing: 0px;
	color: #ffee51;
}

.redpack .center .getnow {
	position: absolute;
	display: block;
	top: 386px;
	left: 107px;
	width: 158px;
	height: 40px;
}

.redpack .center .close {
	position: absolute;
	top: 41px;
	right: 38px;
	cursor: pointer;
}

.getpack {
	width: 100%;
	height: 100%;
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 999;
}

.getpack .cover {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #333;
	filter: Alpha(opacity=7);
	-moz-opacity: 0.7;
	opacity: 0.7;
	z-index: 998;
}

.getpack .content {
	width: 587px;
	height: 523px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -447px;
	margin-left: -293px;
	z-index: 999;
}

.getpack .content .close {
	position: absolute;
	top: 190px;
	right: 105px;
	cursor: pointer;
}

.getpack .content .discounts {
	position: absolute;
	top: 387px;
	left: 130px;
}

.getpack .content .discounts .single {
	width: 321px;
	height: 92px;
	position: relative;
}

.getpack .content .discounts .single:nth-child(1) {
	background: url("../../../frontV1/img/pic/component/priceback10.png") top center;
}

.getpack .content .discounts .single:nth-child(2) {
	background: url("../../../frontV1/img/pic/component/priceback20.png") top center;
}

.getpack .content .discounts .single:nth-child(3) {
	background: url("../../../frontV1/img/pic/component/priceback40.png") top center;
}

.getpack .content .discounts .single .usenow {
	width: 58px;
	height: 22px;
	border-radius: 10px;
	text-align: center;
	border: solid 2px #ef3219;
	font-size: 11px;
	font-weight: normal;
	font-stretch: normal;
	line-height: 19px;
	letter-spacing: 0px;
	color: #ef3219;
	position: absolute;
	right: 22px;
	top: 35px;
	cursor: pointer;
}

.getpack .content .btns {
	margin-top: 150px;
	padding-left: 130px;
	padding-right: 145px;
}

.getpack .content .btns a {
	width: 145px;
	height: 46px;
	background-color: #ffd800;
	border-radius: 5px;
	line-height: 46px;
	text-align: center;
	font-size: 17px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #e60012;
}

.getpack .content .btns .buynow {
	float: left;
}

.getpack .content .btns .check {
	float: right;
}

.getpack .content .notes {
	margin-top: 11px;
	padding-left: 130px;
	padding-right: 145px;
	font-size: 11px;
	font-weight: normal;
	font-stretch: normal;
	line-height: 1.5em;
	letter-spacing: 0px;
	color: #e3e3e3;
}

.getpack .content .notes a {
	color: #ffd800;
}