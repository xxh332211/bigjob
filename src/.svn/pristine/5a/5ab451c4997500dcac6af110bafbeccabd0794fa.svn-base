	font-weight: bold;
}

.nav .nav_container dt .subnav ul:first-child:hover {
	background: #ff7e18;
}

.nav .nav_container dt .subnav ul:first-child:hover a {
	color: #fff;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat {
	display: block;
	opacity: 1;
	transition: width .3s;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat .first {
	color: #333;
	text-align: left;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat a {
	color: #333;
}

.nav .nav_container dt .subnav ul:first-child:hover .second_cat a:hover {
	color: #ff7e18;
}

.nav .nav_container dt .subnav ul .second_cat {
	background: #ffffff;
	box-shadow: 0 1px 4px 1px #eeeeee;
	display: none;
	width: 1000px;
	height: 500px;
	z-index: 999;
	position: absolute;
	overflow-x: hidden;
	overflow-y: scroll;
	left: 200px;
	top: 0;
