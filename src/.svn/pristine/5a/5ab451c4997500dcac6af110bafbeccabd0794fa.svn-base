.nav {
	margin-top: 14px;
	border-bottom: 4px solid #ff7e18;
}

.nav .nav_container dt {
	float: left;
	background: #ff7e18;
	width: 200px;
	height: 40px;
	color: #fff;
	font-size: 16px;
	line-height: 38px;
	text-align: center;
	position: relative;
}

.nav .nav_container dt span {
	vertical-align: middle;
	margin-left: 14px;
}

.nav .nav_container dt .subnav {
	position: absolute;
	top: 44px;
	background: #ffffff;
	box-shadow: 0;
	width: 200px;
	height: 0;
	transition: height .3s;
	z-index: 999;
}

.nav .nav_container dt .subnav ul {
	width: 100%;
	padding: 16px 0 20px 20px;
	border-bottom: 1px dotted #e2e2e2;
	position: relative;
	box-sizing: border-box;
	display: none;
}

.nav .nav_container dt .subnav ul .cat_title {
	line-height: 1em;
	text-align: left;
}

.nav .nav_container dt .subnav ul .cat_title a {
	font-size: 14px;
