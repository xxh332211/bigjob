@import '../../common/bootstrap.min';
@import '../../common/base';
@import '../../common/reset';
@import '../../component/head';
@import '../../component/foot';
@import '../../common/iconfont';
@import '../../common/activity';
.iconfont {
	font-size: 13px;
}
//list_title -s
.list_sup_title {
	height: 30px;
	border: 1px solid #e5e5e5;
	height: 40px;
	line-height: 40px;
	margin-top: 20px;
	span {
		height: 40px;
		&:first-child {
			float: left;
			font-weight: bold;
			font-size: 16px;
			margin-left: 30px;
		}
		&:last-child {
			float: right;
			font-size:12px;
    		color:#888888;
    		margin-right: 15px;
		}
	}
}
//list_title -e
//list_head -s
.list_head {
	.all_result {
		min-height: 50px;
		line-height: 50px;
		margin-top: 1px;
		div {
			float: left;
		}
		.result_title {
			font-size:12px;
			color:#888888;
			margin-right: 15px;
		}
		.result_foot {
			float: right;
			font-size:12px;
			color:#888888;
			margin-right: 15px;
		}
		.result_content {
			min-height: 50px;
			line-height: 50px;
			width: 1040px;
			.result_singel {
				display: inline-block;
				background:#ffffff;
				border:1px solid #e5e5e5;
				padding: {
					left:10px;
					right: 10px;
				}
				height:27px;
				line-height: 27px;
				font-size:12px;
				color:#ff7e18;
				text-align: center;
				cursor: pointer;
				margin-right: 15px;
			}
		}
	}
	.sku_filter {
		.row {
			min-height: 40px;
			border:1px solid #e5e5e5;
			background: #f9f9f9;
			.column_l {
				width: 99px;
				height:100%;
				line-height: 40px;
				text-align: center;
				color:#666;
			}
			.column_r {
				width: 1098px;
				min-height: 40px;
				padding-left: 30px;
				padding-top: 10px;
				padding-bottom: 10px;
				background: #fff;
				&.subfilter {
					padding:0;
					padding-left: 20px;
					.dropdown_ul {
						position: relative;
						.title {
							float: left;
							height: 40px;
							width: 94px;
							text-align: center;
							line-height: 40px;
							cursor: pointer;
							.subfilter_single {
								position: absolute;
								width: 1200px;
								background: #fff;
								left: -120px;
								top:40px;
								display: none;
								border:1px solid #e6e6e6;
								min-height: 40px;
								padding-left: 30px;
								padding-top: 10px;
								padding-bottom: 10px;
								background: #fff;
								z-index: 998;
							}
							&:after {
								width: 100;
								content: '';
								height: 5px;
								background: #fff;
								display: none;
								position: relative;
								top: -2px;
								z-index: 999;
							}
							&.chose_subfilter {
								border: {
									left: 1px solid #e6e6e6;
									right: 1px solid #e6e6e6;
								}
								&:after {
									display: block;
								}
								.subfilter_single {
									display: block;
									.mul_ul {
										.word {
											&:hover {
												border:none;
											}
										}
									}
								}
							}
						}
					}
				}
				.word_ul {
					&.hide_part {
						height: 26px;
						overflow: hidden;
					}
					.word {
						float: left;
						height: 26px;
						line-height: 26px;
						width: 94px;
						text-align: left;
						&.selected {
							a {
								color : #ff7e18;
							}
						}
					}
				}
				.pic_ul {
					width: 921px;
					&.hide_part {
						height: 55px;
						overflow: hidden;
					}
					.pic {
						float: left;
						width: 97px;
						height:55px;
						border: 1px solid #e8e8e8;
						cursor: pointer;
						position: relative;
						img {
							height: 100%;
							margin:0 auto;
							display: block;
							float: none;
						}
						&.selected {
							border-color: #ff7e18;
						}
					}
				}
				.mul_ul {
					width: 921px;
					&.hide_part {
						height: 26px;
						overflow: hidden;
					}
					&.multichose_true {
						.word {
							.icon_cheackbox {
								display: inline-block;
								vertical-align: middle;
							}
						}
					}
					.word {
						float: left;
						height: 26px;
						line-height: 26px;
						width: 94px;
						text-align: left;
						.icon_cheackbox {
							display: none;
							border:1px solid #e5e5e5;
						}
						&.selected {
							a {
								color : #ff7e18;
							}
							.icon_cheackbox {
								background-color: #ff7e18;
							}
						}
					}
				}
				.dropdown_ul {
					.word {
						float: left;
						height: 26px;
						line-height: 26px;
						width: 94px;
						text-align: left;
						.icon_cheackbox {
							display: none;
							border:1px solid #e5e5e5;
						}
						&.selected {
							a {
								color : #ff7e18;
							}
							.icon_cheackbox {
								background-color: #ff7e18;
							}
						}
					}
				}
				.option {
					width: 140px;
					height: 26px;
					line-height: 26px;
					span {
						display: inline-block;
						background:#f5f5f5;
						border:1px solid #e5e5e5;
						width:60px;
						height:26px;
						font-size:12px;
						color:#666666;
						text-align: center;
						line-height: 26px;
						cursor:pointer;
						float: left;
						&:hover {
							color:#ff7e18;
							border:1px solid #ff7e18;
							background: #fff;
						}
						&:nth-of-type(2){
							margin-left: 10px;
							margin-right: 10px;
						}
					}
					.btn_li {
						display: inline-block;
						background:#f5f5f5;
						border:1px solid #e5e5e5;
						width:60px;
						height:26px;
						font-size:12px;
						color:#666666;
						text-align: center;
						line-height: 26px;
						cursor:pointer;
						float: left;
						margin-left: 10px;
						margin-right: 10px;
					}
					.show_more {
						display: inline-block;
						background:#f5f5f5;
						border:1px solid #e5e5e5;
						width:60px;
						height:26px;
						font-size:12px;
						color:#666666;
						text-align: center;
						line-height: 26px;
						cursor:pointer;
						vertical-align: middle;
						&:hover {
							color:#ff7e18;
							border:1px solid #ff7e18;
							background: #fff;
						}
					}
					.mul_chose {
						background:#f5f5f5;
						border:1px solid #e5e5e5;
						width:60px;
						height:26px;
						font-size:12px;
						color:#666666;
						text-align: center;
						line-height: 26px;
						cursor:pointer;
						display: inline-block;
						vertical-align: middle;
					}
				}
			}
		}
	}
	.status_filter {
		margin-top: 20px;
		&.fixtop {
			position: fixed;
			top: 0;
			left: 0;
			right: 0;
			margin:auto;
			width: 1200px;
			z-index: 999;
			.status_filter_top {
				.search {
					display: block;
				}
			}
		}
		.status_filter_top {
			height: 44px;
			background: #f6f6f6;
			border: {
				top:1px solid #e8e8e8;
				bottom:1px solid #e8e8e8;
				right: 1px solid #e8e8e8;
			}
			dl {
				dd {
					padding: 13px 20px;
					font-size:12px;
					height: 42px;
					color:#666666;
					float: left;
					border: {
						left: 1px solid #e8e8e8;
						right: 1px solid #e8e8e8;
					}
					cursor: pointer;
					&.selected {
						background: #ff7e18;
						color: #fff;
					}
				}
			}
			.price {
				height:44px;
				line-height: 44px;
				padding-left: 20px;
				position: relative;
				input {
					display: inline-block;
					background:#ffffff;
					border:1px solid #ededed;
					width:60px;
					height:24px;
					text-align: center;
					font-size:12px;
					color: #666;
				}
				.confirm_log {
					position: absolute;
					left: 10px;
					width: 100%;
					top: 44px;
					margin:auto;
					text-align: center;
					background: #eeeeee;
					z-index: 999;
					.search_cancel {
						background-image:linear-gradient(-180deg, #fefefe 2%, #f3f3f3 100%);
						border:1px solid #cccccc;
						width:49px;
						height:20px;
						cursor: pointer;
					}
					.search_sure {
						background-image:linear-gradient(-180deg, #f56774 0%, #d92943 100%);
						border:1px solid #f11000;
						width:49px;
						height:20px;
						cursor: pointer;
						color: #fff;
					}
				}
			}
			.search {
				width: 500px;
				height: 44px;
				margin-left: 10px;
				padding-top: 5px;
				margin-right: 5px;
				display: none;
				input {
					float: left;
				}
				.search_input {
					width: 430px;
					height: 32px;
					border:2px solid #ff7e18;
					text-indent: 12px;
					color:#666;
				}
				.search_btn {
					width: 70px;
					height:32px;
					background: #ff7e18;
					color: #fff;
					cursor: pointer;
				}
			}
		}
		.status_filter_bottom {
			height: 38px;
			background: #f6f6f6;
			ul {
				li {
					float: left;
					font-size:12px;
					color:#666666;
					height: 38px;
					line-height: 38px;
					cursor: pointer;
					padding : {
						left: 20px;
						right: 20px;
					}
					&.selected {
						span {
							color: #ff7e18;
						}
						i {
							background-color: #ff7e18;
						}
					}
					i {
						display: inline-block;
						vertical-align: middle;
						border: 1px solid #e6e6e6;
					}
					span {
						display: inline-block;
						vertical-align: middle;
						margin-left: 5px;
					}
				}
			}
		}
	}
}
//list_head -e

//list_content -s
.list_content {
	margin-top: 20px;
	.product_single {
		float: left;
		background:#ffffff;
		border:1px solid #eeeeee;
		width:220px;
		height:370px;
		margin-right: 25px;
		margin-bottom: 20px;
		&:nth-of-type(5n) {
			margin-right: 0;
		}
		.pic {
			width: 218px;
			height: 218px;
			&:hover {
				.info_option {
					height: 38px;
				}
			}
			.info_option {
				position: absolute;
				width: 100%;
				line-height: 38px;
				background: pink;
				bottom: 0;
				height:0;
				overflow: hidden;
				transition: all .2s;
				.option_upload {
					width: 50%;
					background: rgba(255,126,24,.9);
					text-align: center;
					font-size:13px;
					color:#ffffff;
					cursor: pointer;
					transition: all .2s;
					&:hover {
						background: rgba(255,60,35,.9);
						transition: all .2s;
					}
				}
				.option_download {
					background: rgba(51,51,51,.9);
					width: 50%;
					font-size:13px;
					color:#ffffff;
					cursor: pointer;
					text-align: center;
					transition: all .2s;
					&:hover {
						background: rgba(255,126,24,.9);
						transition: all .2s;
					}
				}
			}
		}
		.info {
			height:110px;
			width: 218px;
			padding: {
				left: 10px;
				right: 10px;
				top: 10px;
			}
			.price {
				height: 26px;
				line-height: 26px;
				span {
					font-size:20px;
					color:#ff3c23;
				}
				del {
					font-size:12px;
					color:#999999;
					height: 26px;
					margin-left: 10px;
				}
				b {
					font-size:12px;
					color:#999999;
					height: 26px;
					line-height: 26px;
				}
			}
			.title {
				margin-top: 10px;
				width: 200px;
				height: 32px;
				font-size:12px;
				color:#666666;
				overflow: hidden;
				text-overflow: ellipsis;
				display: -webkit-box;
				-webkit-box-orient: vertical;
				-webkit-line-clamp: 2;
				line-height: 1.4em;
				a {
					font-size:12px;
					color:#666666;
					&:hover {
						color: #ff7e18;
					}
				}
			}
			.suplier {
				font-size:12px;
				color:#999999;
				margin-top: 10px;
			}
		}
		.foot {
			height : 40px;
			border-top: 1px solid #eeeeee;
			line-height: 40px;
			span {
				display: inline-block;
				margin-left: 10px;
				height: 18px;
				line-height: 18px;
				box-sizing: content-box;
			}

			.activity {
				font-size:12px;
				color:#ffffff;
				background: #ff7e18;
				padding : 1px 3px;
			}
			.special {
				background:#ffffff;
				border:1px solid #ff7e18;
				width:16px;
				height:16px;
				font-size:10px;
				color:#ff7e18;
				text-align: center;
				line-height: 16px;
			}
		}
	}
}
//list_content -e

//list_foot -s
.list_foot	{
	.guss_u_find {
		height: 42px;
		background:#ffffff;
		border:1px solid #e8e8e8;
		line-height: 42px;
		b {
			font-size:12px;
			color:#666666;
			margin-left: 20px;
		}
		span {
			padding: 0 29px;
			border-left: 1px solid #e5e5e5;
			&:nth-of-type(1) {
				border-left: none;
			}
			a {
				font-size: 12px;
				&:hover {
					color: #ff7e18;
				}
			}
		}
	}
	.guess_u_like {
		margin-top: 20px;
		margin-bottom: 30px;
		.guess_title {
			position: relative;
			background:#fafafa;
			border:1px solid #e5e5e5;
			height:38px;
			line-height: 38px;
			margin-bottom: 20px;
			i {
				background:#ff6600;
				width:6px;
				height:20px;
				display: inline-block;
				float: left;
			}
			span {
				font-size:16px;
				color:#666666;
				font-weight: bold;
				margin-left: 20px;
			}
		}
		.guess_content {
			ul {
				li {
					background:#ffffff;
					border:1px solid #eeeeee;
					width:218px;
					height:298px;
					float: left;
					margin-right: 27px;
					margin-bottom: 21px;
					&:nth-of-type(5n) {
						margin-right: 0;
					}
					.pic {
						width:100%;
						height: 218px;
						background: #666;
						img {
							width: 100%;
							height: 100%;
						}
					}
					.price {
						margin-top: 16px;
						line-height: 20px;
						span {
							font-size:20px;
							color:#ff3c23;
							letter-spacing:0;
							margin-right: 12px;
							float:left;
							margin-left: 12px;
						}
						del {
							font-size:12px;
							color:#999999;
							float:left;
						}
					}
					.title {
						text-align: center;
						margin:0 auto;
						margin-top: 10px;
						white-space:nowrap; 
						overflow:hidden; 
						text-overflow:ellipsis;
						width:194px;
						a {
							font-size:12px;
							color:#666666;
							&:hover{
								color:#ff3c23;
							}
						}
					}
				}
			}
		}
	}
	.list_adv {

	}
}	
//list_foot -e
