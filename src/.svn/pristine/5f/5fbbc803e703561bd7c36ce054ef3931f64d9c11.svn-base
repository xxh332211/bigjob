	height: 100%;
	vertical-align: middle;
	margin-right: -0.25em;
}

.modal_cover .modal_cotainer,
.modal_cover_process .modal_cotainer {
	padding-top: 20px;
	padding-bottom: 20px;
	background: #fff;
	display: inline-block;
	vertical-align: middle;
	border-radius: 5px;
	position: relative;
}

.modal_cover .modal_cotainer .closes,
.modal_cover_process .modal_cotainer .closes {
	position: absolute;
	top: 10px;
	right: 10px;
	padding: 0;
	margin: 0;
	color: #ccc;
	cursor: pointer;
	z-index: 999;
}

.modal_cover .modal_cotainer .closes .iconfont,
.modal_cover_process .modal_cotainer .closes .iconfont {
	border-radius: 8px;
	cursor: pointer;
}

.modal_cover .modal_cotainer div,
.modal_cover_process .modal_cotainer div {
	padding-left: 20px;
	padding-right: 20px;
	margin-top: 20px;
	margin-bottom: 20px;
	text-align: left;
}

.modal_cover .modal_cotainer div.nomatch,
.modal_cover_process .modal_cotainer div.nomatch {
	position: absolute !important;
	bottom: -20px;
	left: 0;
	height: 20px;
	line-height: 20px;
	font-size: 10px;
	display: block;
	color: #ff3110;
}

.modal_cover .modal_cotainer div.nomatch .iconfont,
.modal_cover_process .modal_cotainer div.nomatch .iconfont {
	font-size: 10px;
	vertical-align: middle;
	height: 15px;
	width: 15px;
