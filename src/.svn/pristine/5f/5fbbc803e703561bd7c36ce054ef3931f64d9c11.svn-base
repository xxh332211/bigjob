.foot .foot_top {
	background: #fff;
	height: 202px;
	border-bottom: 4px solid #ff7e18;
	border-top: 2px solid #eee;
}

.foot .foot_top .foot_top_content dl {
	float: left;
	margin-top: 26px;
	margin-right: 59px;
}

.foot .foot_top .foot_top_content dl dt {
	font-size: 16px;
	color: #666666;
	letter-spacing: 0;
	font-weight: bold;
	margin-bottom: 14px;
}

.foot .foot_top .foot_top_content dl dd {
	margin-bottom: 12px;
}

.foot .foot_top .foot_top_content dl dd a {
	font-size: 14px;
	color: #666;
	opacity: 0.8;
}

.foot .foot_top .foot_top_content dl dd img {
	width: 110px;
	height: 110px;
	border: 0;
}

.foot .foot_bottom {
	height: 200px;
	background: #333333;
	color: #fff;
	font-size: 14px;
	padding-top: 15px;
}

.foot .foot_bottom a {
	color: #fff;
	font-size: 14px;
}

.foot .foot_bottom dl dt {
	float: left;
}

.foot .foot_bottom dl dd {
	float: left;
	margin-left: 30px;
}

.foot .foot_bottom ul {
	margin-top: 18px;
}

.foot .foot_bottom ul li {
	float: left;
	padding: 0 20px;
	border-right: 1px solid #fff;
	line-height: 1em;
}

.foot .foot_bottom ul li:first-child {
	padding-left: 0;
}

.foot .foot_bottom ul li:last-child {
	padding-right: 0;
	border-right: 0;
}

.foot .foot_bottom .copyright {
	margin-top: 18px;
}

.foot .foot_bottom .copyright span {
	margin-right: 30px;
}

.foot .foot_bottom .pic {
	width: 114px;
	height: 41px;
	margin-top: 20px;
}

.foot .foot_bottom .pic img {
	width: 100%;
	height: 100%;
}

