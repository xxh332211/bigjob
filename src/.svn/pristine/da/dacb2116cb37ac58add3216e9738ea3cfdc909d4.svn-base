.sidebar_right {
	box-sizing: border-box;
	transition: all 0.5s ease;
	width: 35px;
	height: 100%;
	background: #333333;
	position: fixed;
	right: 0;
	top: 0;
	z-index: 999;
	opacity: 1;
}

.place_holder:hover .sidebar_right {
	box-sizing: border-box;
	display: block;
	width: 35px;
	opacity: 1;
}

.sidebar_right .sidebar_nav {
	box-sizing: border-box;
	position: absolute;
	top: 30%;
	width: 35px;
}

.sidebar_nav li {
	box-sizing: border-box;
	float: left;
	width: 35px;
	height: 30px;
	position: relative;
	text-align: center;
	cursor: pointer;
}

.sidebar_nav li.inpaper {
	height: 132px;
	color: white;
	margin-top: 15px;
}

.sidebar_nav li.inpaper span {
	display: inline-block;
	margin-top: 10px;
	font-size: 14px;
	line-height: 1.5em;
}

.sidebar_nav li.inpaper b {
	background: #ff3c23;
	border-radius: 6px;
	width: 22px;
	height: 16px;
	display: inline-block;
	text-align: center;
	font-size: 12px;
}

.sidebar_nav li.inpaper a i {
	position: relative;
	display: inline-block;
	margin-top: 5px;
}

.sidebar_nav li a i {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}

.sidebar_nav li a i.click {
	display: none;
}

.sidebar_nav li a i.nor {
	display: block;
}

.sidebar_nav li:hover {
	background: #ff7e18;
}

.sidebar_nav li:hover a i.click {
	display: block;
}

.sidebar_nav li:hover a i.nor {
	display: none;
}

.back_top {
	box-sizing: border-box;
	position: fixed;
	bottom: 10px;
	width: 35px;
}

.back_top li {
	box-sizing: border-box;
	float: left;
	height: 30px;
	width: 35px;
	position: relative;
	margin-top: 15px;
}

.back_top li .quickmark {
	background: #fb234a;
	border-radius: 6px;
	width: 0;
	height: 180px;
	position: absolute;
	left: -150px;
	top: -150px;
	overflow: hidden;
	transition: all .2s ease-out;
}

.back_top li .quickmark img {
	width: 158px;
}

.back_top li a i {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}

.back_top li a i.click {
	display: none;
}

.back_top li a i.nor {
	display: block;
}

.back_top li:hover {
	background: #ff7e18;
}

.back_top li:hover .quickmark {
	width: 150px;
	transition: all .2s ease-out;
}

.back_top li:hover a i.click {
	display: block;
}

.back_top li:hover a i.nor {
	display: none;
}

.sidebar_nav .sidebar_r_underline,
.back_top .sidebar_r_underline {
	height: 1px;
	width: 30px;
	margin-left: 3px;
	border-bottom: 0.5px solid #cccccc;
	padding: 0;
}

.sidebar_nav .info_box,
.back_top .info_box {
	box-sizing: border-box;
	overflow: hidden;
	transition: all 0.5s ease;
	float: left;
	position: absolute;
	opacity: 0;
	width: 100px;
	height: 30px;
	text-align: center;
	padding: 7px 0;
	top: 0px;
	left: -200px;
	font-size: 14px;
	font-family: '微软雅黑';
	z-index: 999;
	cursor: pointer;
	color: white;
	width: 0;
}

.sidebar_nav li:hover .info_box,
.back_top li:hover .info_box {
	background: #ff7e18;
	transition: all 0.5s ease;
	opacity: 1;
	left: -100px;
	width: 100px;
}

.back_top ul li:nth-child(2) .info_box {
	background: #ff7e18;
	transition: all 0.5s ease;
	opacity: 1;
	left: -100px;
	width: 100px;
}

