.modal_cover,
.modal_cover_process {
	position: fixed;
	width: 100%;
	height: 100%;
	min-width: 1200px;
	background: rgba(0, 0, 0, 0.5);
	z-index: 999;
	text-align: center;
	line-height: 100%;
	top: 0;
	left: 0;
	overflow: hidden;
}

.modal_cover:before,
.modal_cover_process:before {
	content: '';
	display: inline-block;
	height: 100%;
	vertical-align: middle;
	margin-right: -0.25em;
}

.modal_cover .modal_cotainer,
.modal_cover_process .modal_cotainer {
	padding-top: 20px;
	padding-bottom: 20px;
	background: #fff;
	display: inline-block;
	vertical-align: middle;
	border-radius: 5px;
	position: relative;
}

.modal_cover .modal_cotainer .closes,
.modal_cover_process .modal_cotainer .closes {
	position: absolute;
	top: 10px;
	right: 10px;
	padding: 0;
	margin: 0;
	color: #ccc;
	cursor: pointer;
	z-index: 999;
}

.modal_cover .modal_cotainer .closes .iconfont,
.modal_cover_process .modal_cotainer .closes .iconfont {
	border-radius: 8px;
	cursor: pointer;
}

.modal_cover .modal_cotainer div,
.modal_cover_process .modal_cotainer div {
	padding-left: 20px;
	padding-right: 20px;
	margin-top: 20px;
	margin-bottom: 20px;
	text-align: left;
}

.modal_cover .modal_cotainer div.nomatch,
.modal_cover_process .modal_cotainer div.nomatch {
	position: absolute !important;
	bottom: -20px;
	left: 0;
	height: 20px;
	line-height: 20px;
	font-size: 10px;
	display: block;
	color: #ff3110;
}

.modal_cover .modal_cotainer div.nomatch .iconfont,
.modal_cover_process .modal_cotainer div.nomatch .iconfont {
	font-size: 10px;
	vertical-align: middle;
	height: 15px;
	width: 15px;
	line-height: 15px;
	display: inline-block;
	position: static;
	margin-left: 10px;
	color: #ff3110;
}

.modal_cover .modal_cotainer .title,
.modal_cover_process .modal_cotainer .title {
	font-size: 20px;
	color: #ff7e18;
	margin-bottom: 30px;
	margin-top: 0;
	font-weight: bold;
}

.modal_cover .modal_cotainer .alert,
.modal_cover_process .modal_cotainer .alert {
	padding-top: 10px;
	padding-bottom: 10px;
	font-size: 12px;
	color: #ff3c23;
	background: #f5f5f5;
}

.modal_cover .modal_cotainer .paragraph,
.modal_cover_process .modal_cotainer .paragraph {
	font-size: 12px;
	color: #666666;
	line-height: 1.5em;
	width: 373px;
}

.modal_cover .modal_cotainer .paragraph.big,
.modal_cover_process .modal_cotainer .paragraph.big {
	width: 100%;
}

.modal_cover .modal_cotainer .input,
.modal_cover_process .modal_cotainer .input {
	position: relative;
}

.modal_cover .modal_cotainer .input .nomatch,
.modal_cover_process .modal_cotainer .input .nomatch {
	bottom: -40px;
	left: 65px;
}

.modal_cover .modal_cotainer .input span,
.modal_cover_process .modal_cotainer .input span {
	width: 50px;
	font-size: 12px;
	color: #666666;
	display: inline-block;
	text-align: justify;
	text-align-last: justify;
	margin-right: 5px;
	position: relative;
}

.modal_cover .modal_cotainer .input span > b,
.modal_cover_process .modal_cotainer .input span > b {
	position: absolute;
	top: 0;
	left: -7px;
	color: red;
	font-size: 14px;
}

.modal_cover .modal_cotainer .input input,
.modal_cover_process .modal_cotainer .input input {
	background: #ffffff;
	border: 1px solid #e5e5e5;
	width: 260px;
	height: 30px;
	text-indent: 1em;
	margin-left: 10px;
	font-size: 12px;
	color: #666666;
}

.modal_cover .modal_cotainer .identify,
.modal_cover_process .modal_cotainer .identify {
	position: relative;
}

.modal_cover .modal_cotainer .identify input,
.modal_cover_process .modal_cotainer .identify input {
	width: 200px;
}

.modal_cover .modal_cotainer .identify img,
.modal_cover_process .modal_cotainer .identify img {
	height: 30px;
	width: 50px;
	margin-left: 5px;
	float: none;
	vertical-align: middle;
}

.modal_cover .modal_cotainer .fullinput,
.modal_cover_process .modal_cotainer .fullinput {
	position: relative;
	text-align: center;
}

.modal_cover .modal_cotainer .fullinput div,
.modal_cover_process .modal_cotainer .fullinput div {
	padding: 0;
	margin: 0;
