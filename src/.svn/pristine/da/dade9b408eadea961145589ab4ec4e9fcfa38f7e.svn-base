.modal_cover .modal_cotainer .fullinput .iconfont,
.modal_cover_process .modal_cotainer .fullinput .iconfont {
	position: absolute;
	color: #cccccc;
	font-size: 20px;
	width: 20px;
	height: 20px;
	left: 15px;
	top: 0;
	bottom: 0;
	margin: auto 0;
}

.modal_cover .modal_cotainer .fullinput span,
.modal_cover_process .modal_cotainer .fullinput span {
	display: inline-block;
	width: 80px;
	height: 38px;
	cursor: pointer;
	right: 0;
