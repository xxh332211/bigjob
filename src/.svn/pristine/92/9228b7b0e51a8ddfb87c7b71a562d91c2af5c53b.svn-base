@import '../../common/base';
@import '../../common/reset';
@import '../../component/foot';
@import '../../component/siderbar_right';
@import '../../common/swiper-3.4.2.min';
@import '../../common/iconfont';
@import '../../iconcss/icon_1';
@import '../../component/modal';
body {
	background: #fff;
}
.head_container {
	background: #fff
}
.iconfont {
	font-size: 14px;
	margin-right: 5px;
}
//头部 -s
.head {
    width: 100%;
    height: 30px;
    background:#f5f5f5;
	box-shadow:0 1px 0 0 #eeeeee;
    .head_content {
    	font-size:12px;
		line-height:12px;
		text-align:left;
		//头部左边
    	.head_content_l{
    		dt {
    			float: left;
    			height:100%;
    			line-height:30px;
    			.icon_phone {
    				vertical-align: -3px;
    				margin-right: 5px;
    			}
    		}
    		dd{
    			float: left;
    			height:100%;
    			line-height:30px;
    			margin-left: 20px;
    		}
    	}
		//头部右边
		.head_content_r {
			ul {
				height:30px;
				li {
					float: left;
					height: 30px;
					line-height: 30px;
					padding-left:12px;
					padding-right:12px;
					position: relative;
					.trangle_down {
						vertical-align: -3px;
					}
					.dropdown {
						position: absolute;
						width:100%;
						z-index: 998;
						left:0;
						overflow: hidden;
						display: none;
						background: #fff;
						ul {
							li {
								width:100%;
								float: left;
								a {
									font-size:10px;
									color:#666666;
									&:hover {
										color:#ff7e18
									}
								}
							}
						}
					}
					&:hover {
						background: #fff;
						a {
							color: #ff7e18;
						}
					}
				}
			}
		}
    }
}
//头部 -e

//logo区域 -s
.logo {
	margin-top: 14px;
	height: 80px;
	position: relative;
	.logo_pic {
		width: 350px;
		height:80px;
		position: relative;
		margin-left: 24px;
		.pic_img {
			position: absolute;
			right: 0;
			top: 50%;
			background: #fff;
			margin-top: -12px;
		}
	}
	.logo_search {
		margin-left: 24px;
		height: 80px;
		position: relative;
		width:437px;
		.cover {
			.search {
				box-sizing:content-box;
				color:#999;
				font-size: 12px;
				text-indent: 15px;
				background:#ffffff;
				border:2px solid #da291c;
				border-radius:25px;
				width:437px;
				height:36px;
			}
			.search_btn {
				position: absolute;
				right: 0;
				top: 50%;
				display: block;
				margin-top: -14px;
				i {
					font-size: 24px;
					width: 36px;
					height: 36px;
					line-height: 27px;
				}
			}
		}
	}
	.logo_right{
		height: 80px;
		width: 280px;
		position: relative;
		.cover{
			right: 0;
			.logo_cube {
				margin-left: 10px;
				background:#ffffff;
				border:1px solid #e8e8e8;
				height:38px;
				padding-right: 20px;
				padding-left: 20px;
				font-size:14px;
				color:#666666;
				text-align:center;
				line-height: 38px;
				position: relative;
				cursor: pointer;
				.coverline {
					width:100%;
					height:5px;
					position: absolute;
					z-index: 999;
					bottom: -3px;
					right:0;
					left:0;
					background: #fff;
					display: none;
				}
				.bigdrop {
					position: absolute;
					width: 310px;
					overflow: hidden;
					background: #fff;
					border:0;
					left: -183px;
					top: 37px;
					z-index: 99;
					border:1px solid #e8e8e8;
					display: none;
					.has_cargo {
						max-height: 457px - 38px;
						p{
							font-size:12px;
							height:24px;
							color:#666666;
							line-height:24px;
							text-align:left;
							text-indent: 9px;
							position: absolute;
						    top: 0;
						    left: 0;
							background: #fff;
							width: 100% ;
						}
						.pannel {
							width: 310px;
							height:44px;
							background: #f5f5f5;
							position:absolute;
							bottom: 0;
							line-height: 44px;
							text-align: left;
							span {
								// float: left;
								font-size:12px;
								color:#666666;
								line-height: 1em;
								margin-left: 10px;
								b {
									background: transparent;
									border-radius: 0;
									color: #ff3c23;
									font-weight: bold;
									width:auto;
									line-height: 1em;
								}
							}
							input {
								background:#ff7e18;
								border-radius:4px;
								width:80px;
								height:30px;
								color:#fff;
								float: right;
								margin-top:7px;
								margin-right: 13px;
								cursor: pointer;
							}
						}
						ul {
							overflow-y:auto;
							max-height: 457px - 38px;
							padding-top: 24px;
							padding-bottom: 44px;
							li {
								width:100%;
								height: 68px;
								padding-left: 9px;
								padding-top: 7px;
								background: #fff;
								float: left;
								&:hover {
									background: #f5f5f5;
								}
								.pic {
									width: 52px;
									height: 52px;
									img {
										width: 100%;
										height:100%;
									}
								}
								.product {
									.product_l {
										width: 132px;
										line-height: 1em;
										text-align: left;
										font-size:12px;
										color:#888888;
										margin-left: 6px;
										.title {
											display:block;
											white-space:nowrap; 
											overflow:hidden; 
											text-overflow:ellipsis;
											width: 100%;
											margin-bottom:6px;
										}
										.sku {
											width: 100%;
											margin-bottom: 6px;
										}
										.type {
											width: 100%;
											margin-bottom: 6px;
										}
									}
									.product_r {
										width: 86px;
										line-height: 1em;
										text-align: left;
										font-size:12px;
										color:#888888;
										text-align: right;
										.price {
											width: 100%;
											margin-bottom: 6px;
										}
										.del {
											width: 100%;
											a {
												@extend .product_r;
												&:hover {
													color: #ff3c23;
												}
											}
										}
									}
								}
							}
						}
					}
					.no_cargo {
						width: 100%;
						height: 70px;
						padding-left: 39px;
						line-height: 70px;
						i {
							float: left;
							margin-top: 10px;
						}
						p {
							float: left;
							margin-left: 18px;
							font-family:'NotoSansHans-Regular';
							font-size:12px;
							color:#999999;
						}
					}
				}
				&:hover {
					color: #ff7e18;
					.bigdrop {
						min-height: 70px;
						max-height: 457px - 38px;
						border:1px solid #e8e8e8;
						display: block;
						.no_cargo {
							i {
								float: left;
							}
							p {
								float: left;
								color:#999999;
							}
						}
					}
					.coverline {
						display: block;
					}
				}
				span {
					vertical-align: middle;
				}
				b {
					background:#ff3011;
					width:18px;
					height:18px;
					border-radius:100%;
					color: #fff;
					vertical-align: middle;
					line-height: 18px;
					font-size: 10px;
				}
			}
		}
	}
	
}
//logo区域 -e

//nav区域 -s
.nav {
	margin-top: 14px;
	border-bottom: 4px solid #da291c;
	background:#da291c;
	height:40px;
	.nav_container{
		dt {
			float: left;
			background:#a6201c;
			width:190px;
			height:40px;
			color:#fff;
			font-size: 16px;
			line-height: 38px;
			text-align: left;
			position: relative;
			padding-left: 32px;
			i {
				font-size: 20px;
				width: 20px;
				height: 20px;
				position: absolute;
				right: 15px;
				top: 50%;
				margin-top: -10px;
			}
		}
		dd {
			float: left;
			height: 38px;
			line-height: 38px;
			margin-right:15px;
			margin-left: 15px;
			position:relative;
			.icon_nav {
				width:16px;
				height:16px;
				position: absolute;
				top: 5px;
			}
			a {
				
				font-size:16px;
				color:#fff !important;
				&:hover {
					color:#fff !important;
				}
				&.sparate {
					color:#fff;
				}
			}
			.activity {
				font-size:16px;
				color:#fff !important;
				&:hover {
					color:#fff !important;
				}
			}

			&:nth-child(2) {
				margin-left: 46px;
			}
		}
	}	
}
//nav区域 -e

//banner 区域 -s
.banner {
	height: 410px;
	background: #c71a38;
	margin-top: 10px;
	.banner_left {
		padding-top: 13px;
		height: 410px;
		ul {
			li {
				padding-left: 34px;
				line-height: 42px;
				width: 190px;
				height: 42px;
				&:hover{
					background: #AC0522;
				}
				a {
					font-size:16px;
					color:#ffffff;
				}
			}
		}
	}
	.banner_center {
		width: 742px;
		height: 387px;
		margin-top: 13px;
		background: #fff;
		position: relative;
		.banner_pics {
			width: 100%;
			height: 100%;
			overflow: hidden;
			position: relative;
			.slide_part {
				height: 100%;
				position: absolute;
				width: 10000px;
				top: 0;
				left:0;
				.single {
					float: left;
					height: 100%;
					width: 742px;
				}
			}
		}
		.banner_index {
			position: absolute;
			bottom: 7px;
			width: 100%;
			.main_index {
				width: 100%;
				height: 25px;
				padding-left: 6px;
				text-align: center;
				padding-right: 6px;
				.indexs {
					background: #333;
					height: 25px;
					display: inline-block;
					.index {
						float: left;
						height: 25px;
						width: 146px;
						color: #fff;
						font-size:14px;
						color:#ffffff;
						text-align:center;
						line-height: 25px;
						cursor: pointer;
						&:hover {
							background: #414141;
						}
					}
				}
			}
		}
	}
	.banner_right {
		width: 244px;
		margin-right: 13px;
		height: 410px;
		overflow: hidden;
		.apply_guide_title {
			font-size:24px;
			color:#ffffff;
			text-align:center;
			line-height: 1em;
			margin-top: 21px;
		}
		.apply_guide_step {
			background:#d93751;
			width:244px;
			height:292px;
			padding-left: 10px;
			padding-right: 8px;
			margin-top: 17px;
			.guide_step_content {
				.step {
					.step_title {
						padding-top: 9px;
						padding-bottom:8px;
						line-height: 1em;
						font-size:16px;
						color:#ffffff;
						cursor: pointer;
						border-bottom: 1px solid #f68fa0;
					}
					.step_content {
						width: 226px;
						height: 0;
						background: #fff;
						transition: all .1s linear;
						overflow: hidden;
					}
					&.active {
						.step_title {
							border:none;
						}
						.step_content {
							height: 179px;
							transition: all .1s linear;
						}
					}
				}
			}
			.apply_guide_notice {
				margin-top: 5px;
				p {
					line-height: 1.4em;
					font-size:14px;
					color:#ffffff;
				}
			}
		}
		.apply_guide_btn {
			margin-top: 8px;
			a {
				&:hover {
					div {
						background:#cf314b;
					}
				}
				div {
					background:#da3651;
					width:118px;
					height:38px;
					font-size:16px;
					color:#ffffff;
					text-align:center;
					line-height: 38px;
				}
			}
		}
	}
}
//banner 区域 -e

// recommend -s
.recomend {
	position: relative;
	margin-top: 30px;
	.recomend_box {
		float: left;
		margin-right: 21px;
		&:last-child {
			margin-right: 0;
		}
		.pic {
			width: 284px;
			height: 174px;
			position: relative;
			&:hover {
				.cover_box {
					opacity: 1;
					filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=100);
					transition: all .1s;
				}
			}
			.cover_box {
				width: 100%;
				height: 100%;
				opacity: 0;
				filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
				transition: all .2s;
				position: absolute;
				top: 0;
				left: 0;
				.cover_background {
					width: 100%;
					height: 100%;
					position: absolute;
					top: 0;
					left: 0;
					background: #333333;
					opacity: 0.5;
					filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=50);
					z-index: 2;
				}
				.cover_content {
					position: absolute;
					width: 100%;
					height: 100%;
					top: 0;
					left: 0;
					z-index: 3;
					padding-top: 45px;
					.cover_title {
						text-align: center;
						line-height: 1em;
						font-size:20px;
						color:#ffffff;
					}
					.cover_btn {
						text-align: center;
						margin-top: 20px;
						.btn_content {
							display: inline-block;
							border:1px solid #ffffff;
							width:140px;
							height:38px;
							line-height: 38px;
							font-size:18px;
							color:#f9f9f9;
						}
					}
				}
			}
		}
		.title {
			text-align: center;
			margin-top: 19px;
			a {
				font-size:20px;
				color:#333333;
			}
		}
	}
}
// recommend -e

