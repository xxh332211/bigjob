//today -s
.today {
	margin-top: 33px;
	.today_nav {
		.part {
			width: 159px;
			height: 52px;
			position: relative;
			float: left;
			cursor: pointer;
			border: {
				top: 1px solid #ccd3d9;
				left: 1px solid #ccd3d9;
				bottom: 1px solid #ccd3d9;
			}
			text-align: center;
			line-height: 52px;
			span {
				font-size:16px;
				color:#666666;
				line-height: 1em;
			}
			i {
				position: absolute;
				display: none;
				left: 50%;
				margin-left: -8px;
				bottom: -11px;
				color: #da291c;
			}
			&.active {
				background: #da291c;
				border-color: #da291c;
				span {
					color: #fff;
				}
				i {
					display: block
				}
			}
		}
		.part_2 {
			float: left;
			width: 86px;
			height: 52px;
			border : 1px solid #ccd3d9;
			position: relative;
			.line {
				position: absolute;
				top: 50%;
				left: 50%;
				margin-top: -10px;
				margin-left: -0.5px;
				border-right: 1px solid #ccd3d9;
				height: 21px;
				width: 1px;
			}
			.part_2_l {
				width: 40px;
				height: 52px;
				cursor: pointer;
				&.disable {
					i {
						color: #cbd4d9;
					}
				}
				i {
					width: 100%;
					height: 100%;
					float: left;
					margin: 0;
					font-size: 24px;
					text-align: center;
					line-height: 52px;
					color: #7c7f86;
				}
			}
			.part_2_r {
				width: 40px;
				height: 52px;
				cursor: pointer;
				&.disable {
					i {
						color : #cbd4d9;
					}
				}
				i {
					color: #7c7f86;
					width: 100%;
					height: 100%;
					float: left;
					margin: 0;
					font-size: 24px;
					text-align: center;
					line-height: 52px;
				}
			}
		}
	}
	.today_content {
		padding-left: 25px;
		.content_product {
			display: none;
			&.active {
				display: block;
			}
			.single {
				float: left;
				margin-right: 38px;
				margin-top: 30px;
				&:nth-child(5n) {
					margin-right: 0;
				}
				.pic {
					width: 200px;
					height: 200px;
					position: relative;
					background: black;
					&:hover {
						.upto_ali {
							height: 30px;
							transition: all .1s;
						}
					}
					.upto_ali {
						transition: all .1s;
						cursor: pointer;
						background:#da291c;
						width:200px;
						height:0;
						overflow: hidden;
						position: absolute;
						bottom: 0;
						left: 0;
						text-align: center;
						line-height: 30px;
						span {
							color: #fff;
						}
					}
				}
				.title {
					overflow: hidden;
					text-overflow:ellipsis;
					white-space: nowrap;
					width: 182px;
					margin-top : 16px;
					line-height: 1em;
					a {
						font-size:12px;
						color:#666666;
						&:hover {
							color: #da291c;
						}
					}
				}
				.price {
					margin-top: 20px;
