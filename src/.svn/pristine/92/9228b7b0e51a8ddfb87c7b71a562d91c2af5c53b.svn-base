.brand_sock {
	margin-top: 24px;
	padding-left: 23px;
	padding-bottom: 38px;
	.single {
		float: left;
		margin-right: 38px;
		margin-top: 30px;
		&:nth-child(5n) {
			margin-right: 0;
		}
		.pic {
			width: 200px;
			height: 200px;
			position: relative;
			background: black;
			&:hover {
				.upto_ali {
					height: 30px;
					opacity: 0.8;
					filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=80);
					transition: all .1s;
					&:hover {
						opacity: 1;
						filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=100);
					}
				}
			}
			.upto_ali {
				transition: all .1s;
				cursor: pointer;
				background:#da291c;
				width:200px;
				height:0;
				overflow: hidden;
				position: absolute;
				bottom: 0;
				left: 0;
				text-align: center;
				line-height: 30px;
				span {
					color: #fff;
				}
			}
		}
		.title {
			overflow: hidden;
			text-overflow:ellipsis;
			white-space: nowrap;
			width: 182px;
			margin-top : 16px;
			line-height: 1em;
			a {
				font-size:12px;
				color:#666666;
				&:hover {
					color: #da291c;
				}
			}
		}
		.price {
			margin-top: 20px;
			line-height: 1em;
			span {
				font-size:20px;
				color:#333333;
				b {
					font-size:18px;
					color:#333333;
				}
			}
			del {
				font-size:12px;
				color:#999999;
				margin-left: 10px;
				line-height: 1em;
			}
		}
	}
}
