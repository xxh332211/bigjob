.fl {
    float: left;
}
.fr {
    float: right;
}
.clearfix:after,
.clearfix:before {
    content: "";
    line-height: 0;
    height: 0;
    display: block;
    visibility: hidden;
    clear: both;
}
.clearfix {
    *zoom:1;
}
.banxin {
    width: 1200px;
    margin: 0 auto;
    border:none;
}
.middle {
    position: absolute;
    top: 50%;
    transform:translateY(-50%);
    z-index: 99;
}
.center {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    z-index: 99;
}
.hide {
    display: none;
}
.show {
    display: block;
}
//css简单交互动画
.scal {
    overflow: hidden;
    position: relative;
    img {
        width: 100%;
        height: 100%;
        transition : width .3s ease-out;
        position: absolute;
        top: 50%;
        left:50%;
        transform:translate(-50%,-50%);
    }
    &:hover {
        img {
            width: 110%;
            transition: width .3s ease-out;
        }
    }
}

.jump {
    position: relative;
    top:0;
    transition: all .1s;
    &:hover {
        top: -10px;
        transition: all .1s ease-out;
    }
}
