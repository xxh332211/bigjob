@import '../../common/base';
@import '../../common/reset';
@import '../../common/iconfont';
body {
    background: #f8f4f4;
}
.pic_1 {
    height: 710px;
    min-width: 1200px;
}

.pic_2 {
    height: 967px;
    min-width: 1200px;
}

.pic_3 {
    height: 710px;
    min-width: 1200px;
}

.pic_4 {
    height: 1124px;
