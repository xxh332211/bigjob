.open_appointment {
    position: fixed;
    left: -220px;
    bottom: 0;
    width: 220px;
    height: 120px;
    cursor: pointer;
    overflow: hidden;
}

.holder {
    height: 150px;
}
.side_right {
    position:fixed;
    right:0;
    top:0;
    z-index:999;
    height: 100%;
    width: 3px;
    background-color: #da3651;
    .holder {
        user-select:none;
        width: 23px;
        height: 236px;
        position: absolute;
        top:50%;
        margin-top: -118px;
        right:3px;
        cursor:pointer;
        font-size: 14px;
        font-weight: 300;
        text-align: center;
        line-height: 1.71;
        padding-top:63px;
        color: #fefefe;
        .iconfont {
            margin-right: -4px;
        }
    }
    .appoint_win {
        position: absolute;
        top:50%;
        margin-top: -118px;
        right:-163px;
        width: 163px;
        height: 236px;
        border-radius: 20px;
        .close {
            position: absolute;
            color:#fff;
            top:17px;
            right:14px;
            cursor:pointer;
        }
        p {
            font-size: 30px;
            line-height: 0.93;
            text-align: center;
            color: #ffffff;
            &:nth-child(2){
                margin-top:50px;
            }
        }
        .phoneinput {
            width: 129px;
            height: 33px;
            border-radius: 16.5px;
            background-color: #ffffff;
            text-indent:14px;
            font-size: 12px;
            font-weight: 300;
            line-height: 0.9;
            text-align: left;
            margin-top:28px;
            margin-left:17px;
            box-shadow: inset 0px 2px 5px 0 rgba(186, 114, 114, 0.46);
        }
        .sign_btn {
            width: 128px;
            height: 27px;
            background:transparent;
            border-radius: 13.5px;
            border: solid 1px #ffffff;
            color:#fff;
            cursor:pointer;
            margin-top:15px;
            margin-left:17px;
        }
        .btn {
            position: absolute;
            bottom:32px;
            right:35px;
            color:#fff;
        }
    }
}