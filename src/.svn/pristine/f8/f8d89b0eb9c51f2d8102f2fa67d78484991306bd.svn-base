.special_ser {
    .title {
        width: 100%;
        text-align: center;
        margin-top: 70px;
        p {
            font-size: 48px;
            text-align: center;
        }
        span {
            display: block;
            margin: 0 auto;
            background: #ff5500;
            width: 82px;
            height: 4px;
            margin-top: 20px;
        }
    }
    .content {
        margin-top: 100px;
        .content_left {
            .pic_cover {
                width: 600px;
                height: 313px;
                overflow: hidden;
                position: relative;
                .slide {
                    height: 313px;
                    width: 10000px;
                    position: absolute;
                    top: 0;
                    left: 0;
                    .single {
                        float: left;
                    }
                }
            }
            .base_bottom {
                width: 600px;
                height: 170px;
                text-align: center;
                img {
                    display: inline-block;
                }
            }
        }
        .content_right {
            .indexs {
                .index {
                    margin-bottom: 30px;
                    cursor: pointer;
                    &:last-child {
                        margin-bottom: 0px;
                    }
                    &.active {
                        .no_left {
                            span {
                                background: #ff5500;
                                border: 1px solid #ff5500;
                                color: #fff;
                            }
                        }
                    }
                    .no_left {
                        width: 35px;
                        float: left;
                        margin-right: 20px;
                        span {
                            display: block;
                            background: #ffffff;
                            border: 1px solid #bdbdbd;
                            width: 33px;
                            height: 33px;
                            text-align: center;
                            line-height: 33px;
                            border-radius: 100%;
                            font-size: 14px;
                            color: #333333;
                        }
                    }
                    .text_r {
                        width: 399px;
                        float: right;
                        .text_title {
                            font-size: 26px;
                            color: #333333;
                            text-align: left;
                            line-height: 1em;
                            margin-bottom: 18px;
                        }
                        .text_main {
                            font-size: 22px;
                            color: #666666;
                            text-align: left;
                            line-height: 1.1em;
                        }
                    }
                }
            }
        }
    }
}

.appointment {
    background: #41474d;
    height: 150px;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    overflow: hidden;
    .appointment_content {
        position: relative;
        .close {
            position: absolute;
            color: #dedede;
            font-size: 18px;
            right: -25px;
            top: 16px;
            cursor: pointer;
            -moz-transition: all .5s;
            -webkit-transition: all .5s;
            -o-transition: all .5s;
            -ms-transition: all .5s;
            transition: all .5s;
            &:hover {
                -webkit-transform: rotate(-180deg);
                -moz-transform: rotate(-180deg);
                -o-transform: rotate(-180deg);
                transform: rotate(-180deg);
            }
        }
        .pic {
            float: left;
            img {
                position: relative;
                top: -24px;
            }
        }
        .sign_up {
            float: left;
            overflow: hidden;
            margin-left: 55px;
            .sign_title {
                font-size: 24px;
                color: #ffffff;
                line-height: 24px;
                text-align: left;
                margin-top: 25px;
            }
            .sign_input {
                margin-top: 20px;
                .phoneinput {
                    background: #ffffff;
                    width: 223px;
                    height: 38px;
                    font-size: 14px;
                    color: #bdbdbd;
                    line-height: 38px;
                    text-indent: 11px;
                }
                .sign_btn {
                    background: #ff7600;
                    width: 112px;
                    height: 38px;
                    font-size: 18px;
                    color: #ffffff;
                    line-height: 38px;
                    text-align: center;
                    cursor: pointer;
                }
            }
            .sign_phone {
                font-size: 18px;
                color: #ffffff;
                text-align: left;
                margin-top: 10px;
            }
        }
        .smart_code {
            float: right;
            position: relative;
            margin-top: 16px;
            img {
                float: right;
            }
            span {
                float: right;
                font-size: 18px;
                color: #ffffff;
                line-height: 20px;
                text-align: left;
                margin-left: 10px;
            }
        }
    }
}

.appointment_2 {
    min-width: 1200px;
    height: 711px;
    position: relative;
    .banxin {
        position: relative;
    }
    .input_box {
        position: absolute;
        width: 632px;
        height: 201px;
        top: 282px;
