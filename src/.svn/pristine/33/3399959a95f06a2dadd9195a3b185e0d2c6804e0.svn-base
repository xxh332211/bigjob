@import "../../common/reset.scss";
@import "../../common/base.scss";
@import "../../common/nanoscroller.scss";
.updata {
    margin: 0 auto;
    margin-top: 500px;
    width: 136px;
    position: fixed;
    right: 30px;
    bottom: 100px;
    .updata_pic {
        width: 136px;
        height: 122px;
        background: black;
        cursor: pointer;
        img {
            background: black;
        }
    }
    .updata_window {
        width: 365px;
        height: 267px;
        background: url('../../img/pic/system/updata_bac.png') top center;
        position: absolute;
        left: -380px;
        top: -280px;
        padding:7px;
        padding-top: 61px;
        padding-left: 15px;
        .title {
            width: 318px;
            height: 26px;
            background-color: #eeeeee;
            line-height: 26px;
            text-align: left;
            color: #333333;
            span {
                display: inline-block;
                font-size: 14px;
                font-weight: 500;
                &:nth-child(1){
                    margin-left: 30px;
                    width: 75px;
                }
                &:nth-child(2) {
                    width: 100px;
                }
                &:nth-child(3) {
                    width: 100px;
                }
            }
        }
        .nano {
            background: #bba;
            width: 343px;
            height: 166px;
            position: relative;
            .nano-content {
                padding: 10px;
                .hahaha {
                    height: 1000px;
                }
            }
            .nano-pane {
                position: absolute;
                right: 20px;
                background: #888;
                .nano-slider {
                    background: #111;
                }
            }
        }
    }
}