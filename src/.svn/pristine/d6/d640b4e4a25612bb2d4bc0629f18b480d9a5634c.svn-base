.modal_cover,.modal_cover_process {
	position: fixed;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.5);
	z-index: 999;
	text-align: center;
	line-height: 100%;
	top:0;
	left: 0;
	overflow: hidden;
	&:before {
		  content: '';
		  display: inline-block;
		  height: 100%;
		  vertical-align: middle;
		  margin-right: -0.25em
	}
	.modal_cotainer {
		padding-top: 20px;
		padding-bottom: 40px;
		background: #fff;
		display: inline-block;
		vertical-align: middle;
		border-radius: 5px;
		position: relative;
		.close {
			position: absolute;
			top: 20px;
			right: 10px;
			padding: 0;
			margin :0;
			color: #ccc;
			cursor:pointer;
			z-index: 999;
			.iconfont {
				border-radius: 8px;
				cursor: pointer;
			}
		}
		div {
			padding-left: 20px;
			padding-right: 20px;
			margin-top: 20px;
			margin-bottom: 20px;
			text-align: left;
			&.nomatch {
				position: absolute !important;
				bottom:-20px;
				left: 0;
				height: 20px;
				line-height: 20px;
				font-size:10px;
				display: block;
				color:#ff3110;
				.iconfont {
					font-size: 10px;
					vertical-align: middle;
					height: 15px;
					width: 15px;
					line-height: 15px;
					display: inline-block;
					position: static;
					margin-left: 10px;
					color: #ff3110;
				}
			}
		}
		.title {
			font-size:20px;
			color:#ff7e18;
			margin-bottom: 30px;
			margin-top: 0;
			font-weight: bold;
		}
		.alert {
			padding-top: 10px;
			padding-bottom: 10px;
			font-size:12px;
			color:#ff3c23;
			background: #f5f5f5;
		}
		.paragraph {
			font-size:12px;
			color:#666666;
			line-height:1.5em;
			width: 373px;
		}
		.input {
			span {
				width: 50px;
				font-size:12px;
				color:#666666;
				display: inline-block;
				text-align: justify;
    			text-align-last: justify;
    			margin-right: 5px;
			}
			input {
				background:#ffffff;
				border:1px solid #e5e5e5;
				width:260px;
				height:30px;
				text-indent: 1em;
				margin-left: 10px;
				font-size:12px;
				color:#666666;
			}
		}
		.identify {
			input {
				width: 200px;
			}
			img {
				height: 30px;
				width: 50px;
				margin-left: 5px;
				float: none;
				vertical-align: middle;
			}
		}
		.fullinput {
			position: relative;
			text-align: center;
			div {
				padding:0;
				margin:0;
				position: relative;
			}
			input {
				background:#ffffff;
				border:1px solid #e5e5e5;
				width:330px;
				height:40px;
				text-indent: 65px;
				margin-left: 10px;
				font-size:12px;
				color:#666666;
				margin-left: 0;
			}
			.iconfont {
				position: absolute;
				color: #cccccc;
				font-size: 20px;
				width: 20px;
				height: 20px;
				left: 15px;
				top:0;
				bottom:0;
				margin: auto 0;
			}
			span {
				display: inline-block;
				width: 80px;
				height: 38px;
				cursor:pointer;
				right: 0;
				top: 1px;
				overflow: hidden;
				position: absolute;
				img {
					width: 100px;
				}
			}
		}
		.option {
			margin-bottom: 0;
			margin-top: 30px;
			.btn {
				padding:0;
				margin-bottom: 0;
				.sure {
					float: right;
					border:1px solid #ff7e18;
					padding-top: 10px;
					padding-bottom: 10px;
					cursor: pointer;
					background: #ff7e18;
					font-size:14px;
					color:#ffffff;
					border-radius: 2px;
					margin-top: 0;
					margin-bottom: 0;
				}
				.cancel {
					float: right;
					border:1px solid #e8e8e8;
					padding-top: 10px;
					padding-bottom: 10px;
					cursor: pointer;
					border-radius: 2px;
					margin-top: 0;
					margin-bottom: 0;
					margin-left: 10px;
				}
				.big {
					margin:0 auto;
					width: 330px;
					padding:{
						top : 12px;
						bottom: 12px;
						left:0;
						right: 0;
					}
					text-align: center;
					cursor:pointer;
					background:#ff7e18;
					font-size:14px;
					color:#ffffff;
					border-radius:2px;
					margin-top: 30px;
				}
				.number_chosed {
					float: right;
					margin:0;
					height:36px;
					line-height: 36px;
					font-size:18px;
					color:#333333;
				}
				.login_tag {
					margin-top: 10px;
					padding:0;
					a {
						font-size:12px;
						color:#ff7e18;
						&:first-child {
							float: left;
						}
						&:last-child {
							float: right;
						}
					}
				}
			}
		}

		.process_chose {
			width: 980px;
			min-height: 520px;
			background: #ccc;
			.single {
				width: 220px;
				height: 110px;
				background: #fff;
				float:left;
				padding:8px;
				padding-right: 0;
				border:2px solid transparent;
				margin-right: 15px;
				position: relative;
				cursor: pointer;
				.iconfont {
					position: absolute;
					right: 0;
					bottom:0;
					margin:0;
					width: 30px;
					height: 30px;
					font-size: 30px;
					color: #ff3c23;
					display: none;
				}
				&.selected {
					border: 2px solid #ff3c23;
					.iconfont {
						display: block;
					}
				}
				.pic {
					width: 90px;
					height: 90px;
					padding:0;
					margin:0;
					float: left;
					overflow: hidden;
					img {
						width: 100%;
					}
				}
				.info {
					float:left;
					padding:0;
					margin:0;
					margin-left: 10px;
					font-size:14px;
					color:#666666;
					p {
						&:first-child {
							margin-top: 10px;
						}
						&:nth-of-type(2) {
							margin-top: 17px;
						}
					}
				}
			}
		}
	}
}
