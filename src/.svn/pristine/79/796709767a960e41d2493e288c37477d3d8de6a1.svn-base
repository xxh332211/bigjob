                    <button>-</button>
                    <input type="text" value="0">
                    <button>+</button>
                </div>
                <p>一手=<span>20</span>双</p>
            </div>
            <div class="stock fl">
                库存520双
            </div>
            <div class="second fl">
                <div class="chose clearfix">
                    <i class="iconfont">&#xe640;</i>
                    <span>选择加工方式</span>
                </div>
                <div class="selfbrand clearfix">
                    <i class="iconfont">&#xe640;</i>
                    <span>是否自供商标</span>
                </div>
            </div>
            <div class="add fl">
                添加
            </div>
        </div>
    </div>
    <div class="main_panel all banxin">
        <div class="selectall fl ">
            <i class="iconfont">&#xe640;</i>
            <span>全选</span>
        </div>
        <div class="add_inpaper fr">
            已选<span>1</span>种货品 共<span>50</span>件
            <button>加入进货单</button>
        </div>
    </div>
    <div class="goodstuff banxin">
        <div class="stuff_title">
            好评商品
