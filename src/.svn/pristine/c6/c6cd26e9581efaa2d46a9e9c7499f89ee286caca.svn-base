.banxin {
	width: 1200px;
	margin: 0 auto;
	border: none;
}

.middle {
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	z-index: 99;
}

.center {
	position: absolute;
	left: 50%;
	transform: translateX(-50%);
	z-index: 99;
}

.hide {
	display: none;
}

.show {
	display: block;
}

.scal {
	overflow: hidden;
	position: relative;
}

.scal img {
	width: 100%;
	transition: width .3s ease-out;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.scal:hover img {
	width: 110%;
	transition: width .3s ease-out;
}

.jump {
	position: relative;
	top: 0;
	transition: all .1s;
}

.jump:hover {
	top: -10px;
	transition: all .1s ease-out;
}

.shake {
	overflow: hidden;
	position: relative;
}

.shake img {
	width: 100%;
	height: 100%;
	transition: all .3s ease-out;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.shake:hover img {
	transform: translate(-52%, -50%);
	transition: all .3s ease-out;
}

.cover_show {
	position: relative;
}

.cover_show .pic_cover_all {
	transition: all .4s ease-in;
	width: 100%;
	height: 100%;
}

.cover_show:hover .pic_cover_all {
	opacity: 1;
	transition: all .4s ease-in;
}

.cover_show .pic_cover_all {
	opacity: 0;
}

.cover_show .pic_cover_all .pic_cover {
