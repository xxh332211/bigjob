@import '../../iconcss/icon_1';
//foot -s
.foot {
	.foot_top {
		background: #fff;
		height: 202px;
		border-bottom: 4px solid #ff7e18;
		border-top: 2px solid #eee;
		.foot_top_content {
			dl {
				float: left;
				margin-top: 26px;
				margin-right: 59px;
				dt {
					font-size:16px;
					color:#666666;
					letter-spacing:0;
					font-weight: bold;
					margin-bottom: 14px;
				}
				dd {
					margin-bottom: 12px;
					a {
						font-size:14px;
						color:#666;
						opacity:0.8;
					}
					img {
						width: 110px;
						height: 110px;
						border:0;
					}
				}
			}
		}
	}
	.foot_bottom {
		height:200px;
		background: #333333;
		color:#fff;
		font-size: 14px;
		padding-top: 15px;
		a {
			color:#fff;
			font-size: 14px;
		}
		dl {
			dt{
				float: left;
			}
			dd {
				float:left;
				margin-left: 30px;
			}
		}
		ul {
			margin-top: 18px;
			li {
				float: left;
				padding: 0 20px;
				border-right: 1px solid #fff;
				line-height: 1em;
				&:first-child {
					padding-left: 0;
				}
				&:last-child {
					padding-right: 0;
					border-right: 0;
				}
			}
		}
		.copyright {
			margin-top: 18px;
			span {
				margin-right: 30px;
			}
		}
		.pic {
			width: 114px;
			height:41px;
			margin-top: 20px;
			img {
				width:100%;
				height:100%;
			}
		}
	}
}
//foot -e