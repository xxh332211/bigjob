@import '../../common/bootstrap.min';
@import '../../common/swiper-3.4.2.min';
@import '../../common/base';
@import '../../common/reset';
@import '../../component/head';
@import '../../component/foot';
@import '../../common/iconfont';
.banxin {
	width: 1275px;
}
body {
    background: #fff;
    font-size: 14px;
    color:#000;
    margin: 0;
    padding: 0;
}
.swiper-container {
	position: fixed;
    width: 100%;
    height: 100%;
}
#o-c {
    width: 100%;
    height: 100%;
	margin: 0 auto;
}
.swiper-slide {
    text-align: center;
    font-size: 18px;
    background: #fff;
    
    /* Center slide text vertically */
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}
#i-c1 .swiper-slide{
	height: auto;
}

.main_content {
	text-align: left;
	display: block;
}
.banner {
width: 100%;
height: 815px;
position: relative;
text-align: center;
box-shadow: 0px 15px 35px #d3d3d3;
/* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#f58672+0,ffba16+100;xxh */
background: rgb(245,134,114); /* Old browsers */
background: -moz-linear-gradient(81deg, rgba(245,134,114,1) 20%, rgba(255,186,22,1) 100%); /* FF3.6-15 */
background: -webkit-linear-gradient(81deg, rgba(245,134,114,1) 20%,rgba(255,186,22,1) 100%); /* Chrome10-25,Safari5.1-6 */
background: linear-gradient(81deg, rgba(245,134,114,1) 20%,rgba(255,186,22,1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f58672', endColorstr='#ffba16',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */
	.bannerpic {
		height: 100%;
		width: 100%;
		opacity: .2;
		position: absolute;
		top: 50 ;
		left: 0;
		z-index: 0;
	}
	.banner_center {
		z-index: 999;
		user-select: none;
		color:#fff;
		margin: 0 auto;
		text-align: center;
		// overflow: hidden;
		&>.iconfont_2 {
			color:#fff;
			width: 105px;
			height: 73px;
			font-size: 73px;
			margin-top: 125px;
			text-align: center;

		}
		.title {
			.title_big {
				font-size: 76px;
				text-align: center;
				letter-spacing: 13px;
				line-height: 1em;
				margin-top: 70px;
			}
			.title_mall {
				margin-top: 28px;
				overflow: hidden;
				p{
					font-size: 24px;
					margin-top: 20px;
					text-align: center;
					letter-spacing: 5px;
					font-weight: 100;
					line-height: 1em;
				}
			}
		}
		.alitong_btn {
			margin-top: 86px;
			position: relative;
			button {
				height: 52px;
				width: 179px;
				line-height: 52px;
				text-align: center;
				border: 1px solid #fff;
				border-radius: 5px;
				background: transparent;
				outline: none;
				font-size: 18px;
				letter-spacing: 3px;
				&:hover {
					background: rgba(255,255,255,.2);

				}
			}
			.line {
				position: absolute;
				top: 53px;
				left:50%;
				height: 206px;
				width: 1px;
				border-right: 1px solid #fff;
			}
		}
	}
}


