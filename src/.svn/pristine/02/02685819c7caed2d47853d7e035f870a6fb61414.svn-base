@import '../../common/iconfont';
//banner -s
.banner {
	.banner_pic {
		min-width: 1200px;
		height: 440px;
	}
	.banner_pic_down {
		min-width: 1200px;
		height: 180px;
	}
}
//banner -e

//activity_title -s
.activity_title {
	margin-top: 30px;
	margin-bottom: 10px;
	text-align:center;
	.activity_maintitle {
		font-size:42px;
		color:#000000;
		font-weight: bold;
		line-height: 42px;
		span {
			color: #ff060d;
		}
	}
	.activity_subtitle {
		height: 20px;
		line-height: 20px;
		margin-top: 16px;
		b {
			display: inline-block;
			height: 20px;
			width: 460px;
			background: #e5e5e5;
			vertical-align: middle;
		}
		span {
			display: inline-block;
			height: 20px;
			vertical-align: middle;
			font-size:18px;
			color:#666666;
			padding : {
				left: 25px;
				right: 25px;
			}
		}
	}
}
//activity_title -e

//activity_merchant -s
.activity_merchant {
	margin-bottom: 20px;
	.merchant_single {
		width: 190px;
		height: 96px;
		position: relative;
		margin-right: 11px;
		margin-top: 12px;
		&:nth-child(6) {
			margin-right: 0;
		}
	}
}
//activity_merchant -e

//activity_rank -s
.activity_rank {
	.activity_rank_single {
		&:first-child {
			margin-right: 15px;
		}
		overflow: hidden;
		background: #fff;
		width: 390px;
		height: 440px;
		box-shadow: 0 2px 4px 0 #e2e2e2;
		margin-top: 20px;
		.activity_rank_title {
			.rank_title_top {
				height:10px;
				background: #333;
			}
			.rank_title_word {
				height: 70px;
				border-bottom: 1px solid #e5e5e5;
				text-indent: 24px;			
				line-height: 60px;
				font-size: 18px;
				color: #333;
				font-weight: bold;
				.seemore {
					a {
						font-size:12px;
						color:#333333;
						font-weight: normal;
						margin-right: 20px;
						&:hover {
							color: #ff3c23;
						}
					}
				}
			}
		}
		.activity_rank_content {
			ol {
				width: 100%;
				li {
					height: 70px;
					border-bottom: 1px solid #e2e2e2;
					float: left;
					width: 100%;
					&:first-child {
						.rank_single_l {
							i {
								background: url("../../img/pic/activity/icon_top1.png") top center;
								color: #fff;
								font-size: 24px;
								font-weight: bold;
							}
						}
						.rank_single_r {
							.title {
								a {
									color:#e9b82e;
								}
							}
						}
					}
					&:nth-child(2) {
						.rank_single_l {
							i {
								background: url("../../img/pic/activity/icon_top2.png") top center;
								color: #fff;
								font-size: 24px;
								font-weight: bold;
							}
						}
						.rank_single_r {
							.title {
								a {
									color:#89a7b8;
								}
							}
						}
					}
					&:nth-child(3) {
						.rank_single_l {
							i {
								background: url("../../img/pic/activity/icon_top3.png") top center;
								color: #fff;
								font-size: 24px;
								font-weight: bold;
							}
						}
						.rank_single_r {
							.title {
								a {
									color:#ac7d6b;
								}
							}
						}
					}
					&:nth-child(5n) {
						height: 80px;
						.rank_single_l {
							height:80px;
						}
					}
					.rank_single_l {
						width: 60px;
						height: 70px;
						border-right: 1px solid #e2e2e2;
						line-height: 70px;
						text-align: center;
						i {
							display: inline-block;
							width: 40px;
							height: 40px;
							line-height: 30px;
							font-size:20px;
						}
					}
					.rank_single_r {
						width: 329px;
						.pic {
							width: 60px;
							height: 60px;
							margin-left: 11px;
							margin-top: 4px;
							display: inline-block;
							vertical-align: middle;
							overflow: hidden;
						}
						.title {
							display: inline-block;
							vertical-align: middle;
							margin-left: 15px;
							width: 178px;
							overflow: hidden;
							text-overflow:ellipsis;
							white-space: nowrap;
							a {
								font-size:14px;
								color:#666;
								line-height:14px;
							}
						}
					}
				}
			}
		}
	}
}
//activity_rank -e

//filter -s
.filter {
	.filter_top {
		background: #fff;
		width:100%;
		height:50px;
		line-height: 50px;
		.filter_top_content {
			padding-left: 78px;
			dl {
				dt,dd {
					height: 50px;
					float: left;
					margin-right: 50px;
					a {
						font-family:'NotoSansHans-Regular';
						font-size:16px;
						color:#333333;
						&:hover {
							color: #ff3c23;
						}
					}
				}
			}
		}
	}
	.filter_bottom {
		background: #fff;
		height:50px;
		text-align: left;
		line-height: 50px;
		padding-left: 78px;
		ul {
			float: left;
			li {
				float: left;
				height: 50px;
				margin-right: 50px;
				a {
					font-family:'NotoSansHans-Regular';
					font-size:14px;
					color:#333;
					&:hover {
						color: #ff3c23;
					}
				}
			}
		}
		input {
			display: inline-block;
			border:none;
			&.price {
				background:#ffffff;
				border:1px solid #ededed;
				width:58px;
				height:22px;
				font-size:12px;
				text-indent: .5em;
				&:-webkit-input-placeholder {
					color:#bdbdbd;
				}
			}
			&.price_sure_nor {
				background:#ffffff;
				border:1px solid #ff3c23;
				width:48px;
				height:22px;
				color:#ff3c23;
				cursor: pointer;
			}
			&.price_sure_click {
				background:#ff3c23;
				border:1px solid #ff3c23;
				width:48px;
				height:22px;
				color:#fff;
				cursor: pointer;
			}
			&.search_input {
				float: left;
				height: 24px;
				width: 282px;
			}
			&.btn {
				float: left;
				background:#e5e5e5;
				width:70px;
				height:30px;
				cursor: pointer;
			}
		}
		.search {
			margin-top: 10px;
			background:#ffffff;
			border:2px solid #e5e5e5;
			width:356px;
			height:32px;
			line-height: 28px;
			margin-right: 22px;
			position: relative; 
			text-align: center;
			i {
				position: absolute;
				top: 6px;
				right: 25px;
				cursor:pointer;
			}
		}
	}
}
//filter -e

//brand -s
.content_brand {
	margin-top: 40px;
	.brand_single {
		margin-right: 12px;
		height: 208px;
		&:nth-child(5n) {
			margin-right: 0;
		}
		.activity_brand_pic {
			border:1px solid #e8e8e8;
			width:228px;
			height:114px;
			position: relative;
			overflow: hidden;
			img {
				position: absolute;
				margin:auto;
				top: 0;
				bottom: 0;
				left:0;
				right: 0;
			}
			
		}
		.brand_name {
			text-align: center;
			font-family:'NotoSansHans-Medium';
			margin-top: 20px;
			padding-bottom: 50px;
			a {
				font-size:18px;
				color:#666666;
			}
		}
	}
}
//brand -e


//activity_adv_little -s
.activity_adv_little {
	background:#ffffff;
	width:1200px;
	height:86px;
	margin-top: 22px;
	img {
		width: 100%;
		height: 100%;
	}
}
//activity_adv_little-e

//activity_product -s
.activity_product {
	margin-bottom: 60px;
	.product {
		float: left;
		background:#ffffff;
		border:2px solid #ebebeb;
		width:282px;
		height:430px;
		margin-right: 24px;
		margin-top: 20px;
		&:nth-child(4n) {
			margin-right: 0;
		}
		&:hover {
			border-color: #ff9c00;
		}
		.product_top {
			width: 278px;
			height: 278px;
			position: relative;
			.product_pic {
				width: 278px;
				height:278px;
				position : relative;
				overflow: hidden;
				.wrap-pic_tip {
					position: absolute;
					top: 0;
					left:20px;
					background: #ff8517;
					width: 50px;
					height: 50px;
					font-family:'NotoSansHans-Medium';
					font-size:16px;
					color:#ffffff;
					padding:8px;
					line-height: 1em;
					position: absolute;
					z-index: 998;
				}
				img {
					position: absolute;
					top: 0;
					left: 0;
					right: 0;
					bottom:0;
					margin:auto;
					width: 278px;
					height: 278px;
					z-index: 997;
				}
			}
			.product_notice {
				position: absolute;
				bottom: 0;
				left: 0;
				background:rgba(51,51,51,.6);
				width:280px;
				height:28px;
				p {
					height:28px;
					line-height: 28px;
					color: #fff3f7;
					span {
						margin-left: 12px;
						margin-right: 10px;
					}
				}
			}
		}
		.product_bottom {
			padding-left: 10px;
			padding-top: 10px;
			.product_title {
				height: 40px;
				width: 258px;
				overflow:hidden;
				text-overflow:ellipsis;
				display:-webkit-box;
				-webkit-box-orient:vertical;
				-webkit-line-clamp:2;
				line-height: 1em;
				a {
					font-size: 14px;
					color:#333;
					line-height: 1.5em;
					height:100%;
					&:hover {
						color:#f79625;
					}
				}
			}
			.product_price {
				margin-top: 6px;
				position: relative;
				.price_now {
					b {
						font-family:'NotoSansHans-Regular';
						font-size:18px;
						color:#ff4e00;
					}
					span {
						font-family:'NotoSansHans-Medium';
						font-size:36px;
						color:#ff4e00;
						line-height: 1em;
					}
				}
				.price_origin {
					position: absolute;
					bottom: 0;
					left: 100px;
					b {
						font-family:'NotoSansHans-Regular';
						font-size:18px;
						color:#8a8986;
					}
					span {
						font-family:'NotoSansHans-Medium';
						font-size:24px;
						color:#8a8986;
					}
					.del_line {
						border-bottom: 1px solid #8e8986;
						position: absolute;
						width: 100%;
						top:50%;
						transform:translateY(-50%);
					}
				}
				.price_supplier {
						font-family:'MicrosoftYaHei';
						font-size:14px;
						color:#999999;
						position: absolute;
						top:50%;
						transform:translateY(-50%);
						right: 10px;
				}
			}
			.product_btn {
				margin-top:5px;
				overflow: hidden;
				.buynow {
					background:#ff9c00;
					width:260px;
					height:40px;
					color:#fff;
					font-size: 14px;
					cursor: pointer;
				}
			}
		}
	}
}
//product_adv_middle -s
.product_adv_middle {
	position: relative;
	height: 56px;
	img {
		position: absolute;
		left: 50%;
		transform:translateX(-50%);
		height:100%;
	}
}
//product_adv_middle -e

//product_more -s
.product_more {
	margin-top: 20px;
	margin-bottom: 60px;
	ul {
		li {
			width:190px;
			height:236px;
			float: left;
			margin-right:12px;
			&:last-child {
				margin-right:0;
			}
			img {
				width: 100%;
				height: 100%;
			}
		}
	}
}
//product_more -e

//page -s
.page {
	margin-bottom: 60px;
	.page_click {
		line-height: 32px;
		margin-right: 5px;
		.allpage {
			font-size :14px;
			color: #999;
		}
		.prv{
			background:#ffffff;
			border:1px solid #cccccc;
			width:80px;
			height:32px;
			line-height: 32px;
			font-size: 10px;
			text-align: center;
			margin-right: 5px;
			a {
				display: inline-block;
				width: 100%;
				height: 100%;
				position: relative;
				.iconfont {
					font-size: 10px;
					position: absolute;
					top:11px;
					left:10px;
				}
			}
		}
		ul {
			height:32px;
			line-height: 32px;
			margin-right: 5px;
			li {
				float: left;
				a {
					float: left;
					display: block;
					background:#ffffff;
					border:1px solid #cccccc;
					border-left:none;
					line-height: 32px;
					font-size: 12px;
					text-align: center;
					width:38px;
					height:32px;
					&.selected {
						background: #ff3c23;
						color:#fff;
					}
				}
			}
		}
		.next {
			background:#ffffff;
			border:1px solid #cccccc;
			width:80px;
			height:32px;
			line-height: 32px;
			font-size: 10px;
			text-align: center;
			a {
				display: inline-block;
				width: 100%;
				height: 100%;
				position: relative;
				.iconfont {
					font-size: 10px;
					position: absolute;
					top:11px;
					right:6px;
				}
			}
		}
	}
	.page_chose {
		line-height: 32px;
		height: 32px;
		color: #999;
		.page_input {
			width: 45px;
			height:25px;
			text-align: center;
		}
		.page_sure {
			background:#ffffff;
			border:1px solid #cccccc;
			width:38px;
			height:32px;
			cursor: pointer;
			color: #666;
		}
	}
}
//page -e