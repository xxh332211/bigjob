define(['jquery','common',"bootstrap","alitong_operate","modal","swipper"],function($,common,bootstrap,alitong_operate,modal,swipper){
    //关闭侧边栏
    $('.side_adv .close').on('click', function(event) {
        $('.side_adv').addClass('hide');

    });

		//轮播图
		var mySwiper = new Swiper('.swiper-container', {
			loop: true,
			pagination: '.swiper-pagination',
			paginationClickable: true,
			nextButton: '.swiper-button-next',
			prevButton: '.swiper-button-prev',
			speed:600,
			autoplay : 3000
		});
});